}

@article{ WOS:A1988T628700008,
Author = {PUNJ, V and GUPTA, RP},
Title = {VA-MYCORRHIZAL FUNGI AND RHIZOBIUM AS BIOLOGICAL FERTILIZERS FOR
   LEUCAENA-LEUCOCEPHALA},
Journal = {ACTA MICROBIOLOGICA POLONICA},
Year = {1988},
Volume = {37},
Number = {3-4},
Pages = {327-336},
Abstract = {Respnse of Leucaena leucocephala to dual inoculation with mycorrhizal
   fungus Glomus fasciculatum and two efficient strains of Rhizobium sp. R4
   and R5 in combination with added phosphorus and nitrogen fertilizers was
   studied in P-deficient sterile soil. Mycorrhizal application resulted in
   significant increase in the symbiotic parameters, which were on par with
   added phosphorus (300 kg super phosphate/ha). Increase in the parameters
   was also observed with Rhizobium application. Dual inoculation improved
   growth, nodulation and nitrogen fixation further as compared to single
   inoculation of either organism which was also evidenced by the increase
   in accumulation of nitrogen and phosphorus in plant parts. Dual
   inoculation was found to be capable of fulfilling the requirements of
   phosphorus and nitrogen in plants.},
ISSN = {0001-6195},
Unique-ID = {WOS:A1988T628700008},
}

@article{ WOS:000498801700003,
