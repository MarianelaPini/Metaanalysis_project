   Mejia-Saenz, E. and Chavez, L. Tijerina and Pinon, M. Delgadillo and
   Larque-Saavedra, U. and Verduzco, C. Villanueva and Gardezi, Habibshah},
Editor = {Callaos, N and Lesso, W and Zinn, CD and Baralt, J and Yoshida, E},
Title = {GROWTH RESPONSE OF LEUCAENA LEUCOCEPHALA ASSOCIATED WITH
   ARBUSCULAR ENDOMYCORRHIZAE TO APPLICATIONS OF ORGANIC MATTER IN SOIL
   IRRIGATED WITH SEWAGE WATER},
Booktitle = {WMSCI 2008: 12TH WORLD MULTI-CONFERENCE ON SYSTEMICS, CYBERNETICS AND
   INFORMATICS, VOL IV, PROCEEDINGS},
Year = {2008},
Pages = {153+},
Note = {12th World Multi-Conference on Systemics, Cybernetics and
   Informatics/14th International Conference on Information Systems
   Analysis and Synthesis, Orlando, FL, JUN 29-JUL 02, 2008},
Organization = {Int Inst Informat \& System; Int Federat Syst Res},
Abstract = {In the irrigation District 018, Tulancingo, Hidalgo, Mexico forage for
   cattle is irrigated with residual water. To evaluate the level of
   contamination of two plots, one hectare was irrigated with residual
   water and the other with clean water. Later a greenhouse experiment with
   Leucaena leucocephala (guaje) to observe its growth with inoculation of
   Glomus intraradices with different amounts of vermicompost was
   esatblished. A factorial experiment 7x2 completely randomized design
   with five replications in the greenhouse was used. The variables
   measured were plant height, stem diameter, root volume dry weight of
   biomass, dry weight of roots. The experiment lasted 180 days from
   planting until harvesting. It is concluded that the inoculation with
   Glomus intraradices increased the efficiency of Leucaena leucocephala in
   nutrient adsorption. In soil the contamination of Cu, Cr and Ni are
   below the established limits for contaminants. Pb was not found. A
   comparison of soil irrigated with clean water or sewage water indicated
   that Cu is nearly twice as concentrated in soil irrigated by
   contaminated water; Ni is slightly greater; Cr is more abundant in soil
   irrigated with clean water.},
ISBN = {978-1-934272-34-3},
ORCID-Numbers = {OJEDA TREJO, ENRIQUE/0000-0001-9263-3523},
Unique-ID = {WOS:000264003300034},
}

@article{ WOS:000420317000004,
Author = {Hincapie Ladino, Eduard Alexis and Bueno Lopez, Liliana and Torres
   Osorio, Javier Ignacio},
Title = {Magnetic Stimulation of Seed of Leucaena Leucocephala and its Effect on
   the Morphological Changes},
Journal = {ENTRE CIENCIA E INGENIERIA},
Year = {2011},
Number = {10},
Pages = {39-47},
Month = {JUL-DEC},
Abstract = {The controlled magnetic stimulation of seeds of Leucaena leucocephala,
   which improves the plant development in several ways, is an important
   methodology to study the contribution of magnetism in the development of
   this legume in silvopastoral systems. Based on criteria for the
   treatment of plant material as defined in the literature, exposure was
   performed with homogeneous static magnetic field. The magnetic flux
   density (30, 62.5, 125, 200 and 250) mT and the exposure time of seeds
   (0,5, 1, 3, 5, 10, 15, 20, 25, 30, 60) minutes were considered as
   exposure parameters. Root length, stem length and leaf mass were
   measured in order to determine the effect of magnetic field on plants 15
   days after germination. An analysis of variance was applied to determine
   significant differences between treatments; in general, variables of
   plant growth from seeds stimulated magnetically were increased, and
   significant differences were found to stem length for treatments 250
   mT/15 min (70,9\%) and for leaf mass the treatment 30 mT/10 min (63,2\%)
   compared to the control group.},
ISSN = {1909-8367},
Unique-ID = {WOS:000420317000004},
}

@inproceedings{ WOS:000082289100042,
Author = {Tolera, A and Seyoum, M and Sundstol, F},
Editor = {Shelton, HM and Gutteridge, RC and Mullen, BF and Bray, RA},
Title = {Nutritive evaluation of Leucaena leucocephala,
   L-diversifolia and L-pallida in Awassa, southern Ethiopia},
Booktitle = {LEUCAENA - ADAPTATION, QUALITY AND FARMING SYSTEMS},
Series = {ACIAR PROCEEDINGS SERIES},
Year = {1998},
Number = {86},
Pages = {261-264},
Note = {Workshop on Leucaena - Adaptation, Quality and Farming Systems, HANOI,
   VIETNAM, FEB 09-14, 1998},
Organization = {Australian Ctr Int Agr Res; Crawford Fund Int Agr Res; Vietnam Natl
   Univ; Univ Queensland, Australia},
Abstract = {The nutritive value of Leucaena leucocephala, L. diversifolia and L.
   pallida foliage was assessed on the basis of chemical composition, in
   vitro gas production and in sacco dry matter (DM) degradability. L.
   leucocephala had the lowest fibre content, but L. pallida had the
   highest crude protein content, and the lowest extractable and total
   proanthocyanidins contents. L. diversifolia had a lower rate of gas
   production than the other two species (p < 0.05) and tended to produce
   relatively less volume of gas after 12 and 24 h of incubation. The other
   gas production constants and the potential gas production of the three
   species were not significantly different. L. leucocephala had higher (p
   < 0.05) DM degradability than L. diversifolia at 4, 8, 16 and 48 h of
   incubation and higher degradability than L. pallida at 8 and 16 h of
   incubation. However, there was no significant difference (p > 0.05) in
   overall DM degradation characteristics of the three species, except that
   L. pallida had a lower washing loss (p < 0.05) than L. diversifolia.},
ISBN = {1-86320-232-3},
ResearcherID-Numbers = {Tolera, Adugna/AAB-3388-2019},
Unique-ID = {WOS:000082289100042},
}

@article{ WOS:A1997WN04400004,
Author = {Angulo, R and Montilla, Y and Rivas, M and Clavero, T and Razz, R and
