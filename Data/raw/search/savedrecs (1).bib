   Araujo, Mariana/E-5434-2014},
ORCID-Numbers = {Oliva, Maria Luiza/0000-0002-5822-0060
   Batista, Isabel/0000-0001-7398-4184
   Araujo, Mariana/0000-0001-5312-5075},
Unique-ID = {WOS:000085998500007},
}

@article{ WOS:A1992KQ63100006,
