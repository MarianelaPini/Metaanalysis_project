Title = {Effect of Sesbania grandiflora or Leucaena leucocephala on
   rumen environment and production of goats fed a basal diet of maize
   stover},
Booktitle = {LEUCAENA - ADAPTATION, QUALITY AND FARMING SYSTEMS},
Series = {ACIAR PROCEEDINGS SERIES},
Year = {1998},
Number = {86},
