@article{ WOS:000449307900044,
Author = {Maneechakr, Panya and Chaturatphattha, Phutthiphong and Karnjanakom,
   Surachai},
Title = {Adsorption behavior of As(V) from aqueous solution by using
   Fe<SUP>3+</SUP>-MnO4<SUP>-</SUP>-modified activated carbon
   (Leucaena leucocephala (Lam) de Wit)},
Journal = {RESEARCH ON CHEMICAL INTERMEDIATES},
Year = {2018},
Volume = {44},
Number = {11},
Pages = {7135-7157},
Month = {NOV},
Abstract = {The objective of this work was to prepare a highly efficient adsorbent
   from Leucaena leucocephala (Lam) de Wit via pyrolysis and chemical
   activation (H3PO4) with a modification (MnO4- and Fe3+) process for
   HAsO42- removal from aqueous solution. It has been found that the
   activated carbon prepared by pyrolysis activation at a temperature of
   500 degrees C for 2h (502) exhibited the best performance for the
   adsorption of the I-2 number (505.69mgg(-1)) and Fe3+ (28.18mgg(-1)).
   After carbonized carbon (CC) and 502 were modified by using MnO4- and
   Fe3+, the HAsO42- adsorption capacity was increased up to 100 times when
   compared with CC and 502 without chemical modification. The pH range of
   about 6-7 was appropriate for Fe3+ and the HAsO42- adsorption. For the
   regeneration of adsorbent, the Fe3+ and HAsO42- could be successfully
   desorbed from the CC surface by using 1.0molL(-1) HNO3 solution. The
   adsorption data were well described by the Langmuir and
   pseudo-second-order kinetic models, which could be confirmed by R-2
   close to 1. Moreover, the adsorption behaviors were physisorption,
   spontaneous and endothermic, confirmed by Themkin and
   Dubinin-Radushkevich isotherms as well as thermodynamic studies. This
   research was expected to help to understand the adsorption behavior of
   As(V) and to provide a suitable process with high adsorption capacity.
   {[}GRAPHICS]
   .},
DOI = {10.1007/s11164-018-3547-1},
ISSN = {0922-6168},
EISSN = {1568-5675},
ORCID-Numbers = {Karnjanakom, Surachai/0000-0001-5130-765X},
Unique-ID = {WOS:000449307900044},
}

@article{ WOS:A1992HK73800008,
Author = {NAIDU, R and SYERS, JK},
Title = {INFLUENCE OF SUGARCANE MILLMUD, LIME, AND PHOSPHORUS, ON SOIL
   CHEMICAL-PROPERTIES AND THE GROWTH OF LEUCAENA-LEUCOCEPHALA IN AN OXISOL
   FROM FIJI},
Journal = {BIORESOURCE TECHNOLOGY},
Year = {1992},
Volume = {41},
Number = {1},
Pages = {65-70},
Abstract = {The effect of lime-containing sugarcane millmud, lime, and phosphorus
   (P), added separately and/or in combination, on soil chemical properties
   and the growth of the tropical legume Leucaena leucocephala was
   investigated in an oxisol from Fiji.  Millmud was added so that the pH
   of the soil was increased from 5.0 to 6.0.  The millmud contained high
   concentrations of several essential major and minor nutrients.  Olsen-
   and water-extractable P in the soil increased with lime and/or millmud
   addition, whereas M KCl-extractable Al decreased to undetectable levels.
   The solution composition of soils treated with millmud showed a
   pronounced increase in all of the major and minor nutrients
   investigated, whereas lime addition only caused an increase in Ca. 
   Plant growth was very low in the unamended soil, even in the presence of
   added P.  However, addition of millmud caused a marked increase in yield
   which was 3-4 times higher than with the soils treated with lime only. 
   Because many of the soils in Fiji are strongly weathered, acidic and
   have a low nutrient status, the result obtained suggests that an
   appropriate use of millmud would prove beneficial to farmers.},
DOI = {10.1016/0960-8524(92)90100-C},
ISSN = {0960-8524},
ResearcherID-Numbers = {Naidu, Ravi/F-5927-2012
   },
ORCID-Numbers = {Naidu, Ravi/0000-0002-1520-2495},
Unique-ID = {WOS:A1992HK73800008},
}

