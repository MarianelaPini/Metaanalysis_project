Unique-ID = {WOS:000326358600024},
}

@article{ WOS:000179930200023,
Author = {Rajendran, D and Pattanaik, AK and Khan, SA and Bedi, SPS},
Title = {Influence of iodine on reproductive performance of bucks fed subabul
   (Leucaena leucocephala) leaf-meal},
Journal = {INDIAN JOURNAL OF ANIMAL SCIENCES},
Year = {2002},
Volume = {72},
Number = {9},
Pages = {802-805},
Month = {SEP},
Abstract = {The effect of supplementation of iodine on male reproduction of goats,
   when their diet contained subabul(Leucaena leucocephala) leaf-meal, was
   assessed using 6 adult bucks (2.5-3 years; 24.4+/-0.94 kg). The bucks
   were randomly divided into 2 groups of 3 animals each, viz., control
   (I-0) and iodine supplemented (I-100) The animals were fed on a
   concentrate supplement along with leucaena leaf-meal (LLM), the latter
   to meet 50\% of their CP requirements. Wheat straw was given ad lib. In
   addition, animals under group I-100 received 100 mug of supplemental
   iodine/head/day. Semen was collected and analysed at monthly intervals
   during the 3 months of experimental feeding. Plasma samples were
   analysed for testosterone at the begining and end of the experiment. The
   results revealed that animals under I-100) group maintained their libido
   throughout the experimental period in contrast to a progressive decline
   observed in the I-0 group. The concentration of spermatozoa (3.25+/-0.39
   vs 2.74+/-0.24 billion ml(-1)) and per cent live spermatozoa
   (76.67+/-1.82 vs 66.17+/-2.00) were significantly (P<0.01) higher in
   iodine supplemented than the non-supplemented bucks (I-100). Other semen
   characteristics, viz. semen volume, pH, number of spermatozoa per
   ejaculate and mass activity although similar (P>0.05), tended to be
   higher in group. The initial fructose content, similarly, tended to be
   higher in I-100. than 10 animals. Mean plasma testosterone level was
   significantly (P<0.05) higher in I-100 than I-0 (1.78+/-0.21 vs:
   1.14+/-0.46 ng ml(-1)). The study indicated that supplementation of
   iodine to leucaena based diet can improve the male reproductive
   performance of goats.},
