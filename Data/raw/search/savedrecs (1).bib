@article{ WOS:000186479400008,
Author = {La O, O and Chongo, B and Delgado, D and Valenciaga, D and Rodríguez, Y
   and Scull, I and Ruiz, TE and Oramas, A},
Title = {Influence of polyethyleneglycol-3500 on the ruminal degradability of
   Leucaena leucocephala cv CIAT-7929},
Journal = {CUBAN JOURNAL OF AGRICULTURAL SCIENCE},
Year = {2003},
Volume = {37},
Number = {3},
Pages = {271-278},
Month = {SEP},
Abstract = {The effect of different levels of polyethyleneglycol (PEG-3500) (0.90
   and 180 g of PEG/kg of DM) oil the in situ ruminal effective
   degradability (ED) of nutrients (DM, CP, NDF and ADF) and on the content
   of condensed tannins bound to the protein (CT-P) and to the fiber (CT-F)
   in Leucaena leucocephala cv CIAT-7929 was Studied. A random block design
   was used with three replications per treatment. Bags were incubated in
   the rumen during 6.81, 12.24.48 and 72 h. The effet of the PEG level oil
   the in vitro intestinal digestibility of nitrogen was determined. There
   were differences (P < 0.01) in the CT-F content. according to the PEG
   level, while the CT-P did not respond to the treatment with
   polyethyleneglycol, There were differences (P < 0.05) between the
   control and the treatment with 90 g in crude protein degradability
   (CPD). The degradability of DM, NDF and ADF varied more (P < 0.01) per
   treatment, independently of the constants of ruminal re-change (k). The
   dvnamics of ruminal degradation had a significant increase, ill regards
   to the incubation time established. This provoked the formation of
   asymptotes between the curves of each indicator in the last hours. There
   were significant differences between the control and the PEG
   concentrations in the in vitro intestinal digestibility of the
   nitrogen., although there were no differences between both treatments.
   Coefficients of determination R-2 > 0.64 were obtained in the linear
   regression equations for the ED of the NDF, ADF and TC-F, according to
   the level of PEG. These results are useful for further studies. in which
   marker techniques may be used to determine the role of the microbial and
   diet nitrogen of this legume.},
