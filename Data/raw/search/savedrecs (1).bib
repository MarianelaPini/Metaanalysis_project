@article{ WOS:000286778400007,
Author = {Pandey, Veda P. and Dwivedi, Upendra N.},
Title = {Purification and characterization of peroxidase from Leucaena
   leucocephala, a tree legume},
Journal = {JOURNAL OF MOLECULAR CATALYSIS B-ENZYMATIC},
Year = {2011},
Volume = {68},
Number = {2},
Pages = {168-173},
Month = {FEB},
Abstract = {Peroxidase was purified to homogeneity from a tree legume Leucaena
   leucocephala. On SDS-PAGE the purified enzyme exhibited two distinct
   subunits each of 66 and 58 kDa. Determination of native molecular weight
   of the purified peroxidase revealed a size of similar to 200 kDa
   suggesting a heterotrimeric structure (consisting of two subunits of 66
   kDa and one subunit of 58 kDa) for native peroxidase. Purified
   peroxidase was found to be a glycoprotein (0.09 mg carbohydrates per mg
   purified peroxidase protein). Purified enzyme exhibited pH optimum of
   5.0 and temperature optimum of 55 degrees C. The Michaelis-Menten
   constants (Km) for guaiacol, H2O2, were found to be 2.9 and 5.6 mM,
   respectively. Divalent cations namely, Ca2+ and Mn2+ activated
   peroxidase at lower concentration (up to 50 mM) while inhibited at
   higher concentration. Monovalent cation namely Na+ did not inhibit
   peroxidase at concentration as high as 4 M. L. leucocephala peroxidase
   was found to be unique as it was not inhibited by azide. (c) 2010
   Elsevier B.V. All rights reserved.},
DOI = {10.1016/j.molcatb.2010.10.006},
ISSN = {1381-1177},
EISSN = {1873-3158},
ResearcherID-Numbers = {Dwivedi, Upendra/AAM-7657-2021
   },
ORCID-Numbers = {PANDEY, VEDA PRAKASH/0000-0002-5856-1519
