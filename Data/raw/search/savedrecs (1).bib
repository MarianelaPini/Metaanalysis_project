@inproceedings{ WOS:000082289100043,
Author = {Rincon, MT and Allison, MJ and Michelangeli, F and Dominguez-Bello, MG},
Editor = {Shelton, HM and Gutteridge, RC and Mullen, BF and Bray, RA},
Title = {Anaerobic degradation of hydroxypyridines from Leucaena
   leucocephala by rumen bacteria},
Booktitle = {LEUCAENA - ADAPTATION, QUALITY AND FARMING SYSTEMS},
Series = {ACIAR PROCEEDINGS SERIES},
Year = {1998},
Number = {86},
Pages = {265-267},
Note = {Workshop on Leucaena - Adaptation, Quality and Farming Systems, HANOI,
   VIETNAM, FEB 09-14, 1998},
Organization = {Australian Ctr Int Agr Res; Crawford Fund Int Agr Res; Vietnam Natl
   Univ; Univ Queensland, Australia},
Abstract = {Some rumen bacteria degrade toxic compounds from Leucaena leucocephala,
   avoiding intoxication problems occurring in non-ruminants after
   ingestion of the plant. Degradation activity by one rumen strain,
   Synergistes jonesii was found to be non-constitutive. The substrate 3,4
   dihydroxypyridine (DHP) induced degradation of both 3,4 and 2,3 DHP
   isomers, while 2,3 DHP induced only degradation of 2,3 DHP. Activity of
   degradation was inhibited by high substrate concentration. The enzyme
   cocktail had a high hydrogenase activity and degraded 2,3 DHP
   anaerobically either in the presence of methyl viologen/H-2 or in the
   presence of alpha-ketoacids (under H-2 or N-2) Specific activity of 2,3
   DHP degradation was increased when FAD+ or CoA were present. The 2,3 DHP
   was found to be an intermediate of the degradation of 3,4 DHP by
   cultures. An unidentified non-polar amino compound appeared in TLC as
   2,3 DHP was degraded.},
ISBN = {1-86320-232-3},
ResearcherID-Numbers = {Rincon, Marco/T-4798-2019},
Unique-ID = {WOS:000082289100043},
}

