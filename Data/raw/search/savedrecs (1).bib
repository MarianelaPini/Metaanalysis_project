@article{ WOS:000182545600011,
Author = {Mesa, D and Romero, A and Cruz, AM},
Title = {Study of different benzylaminopurine (BAP) concentrations in the in
   vitro micropropagation of Leucaena leucocephala cv Peru},
Journal = {CUBAN JOURNAL OF AGRICULTURAL SCIENCE},
Year = {2002},
Volume = {36},
Number = {3},
Pages = {261-264},
Month = {SEP},
Abstract = {A study was undertaken to determine the effect of different
   benzylaminopurine (BAP) concentrations on the in vitro micropropagation
   of Leucaena leucocephala cv Peru. The Murashige and Skoog culture medium
   with vitamins B5 was used. The influence of ten BAP condentrations (1,
   2, 3, 4, 5, 6, 7, 8, 9 and 10 mg/L) and a control treatment without
   hormones on the average height of the plantlets, the percentage of
   plantlets and the number of plantlets/explant was determined by a
   completely randomized design with three repetitions. All the
   concentrations (0-10 mg/L of BAP) showed the same responses in the
   percentage of plantlets with values between 30 and 70 \%. The highest
   average height of the plantlets was obtained with 0, 2, 3, 4 and 9 mg/L
   of BAP-with values between 2.13 and 2.40 cm (P < 0.001), compared to 6
   mg/L,without differences with the rest of the treatments. The number of
   plantlets/explant (2-3) did not vary with the different. concentrations,
   except the control with the smallest (P < 0.001) number. It is concluded
   that, from an economic point of view, a concentration of 1 mg/L should
   be used for the micropropagation of this species.},
ISSN = {0864-0408},
EISSN = {2079-3480},
ORCID-Numbers = {Mesa Travieso, Dianelis/0000-0001-6076-0579},
Unique-ID = {WOS:000182545600011},
}

@article{ WOS:000595532500045,
Author = {Chigurupati, S.},
Title = {PHYTOCHEMICAL SCREENING, ANTIOXIDANT AND CARBOHYDRATE DIGESTING ENZYME
   INHIBITORY POTENTIAL OF LEUCAENA LEUCOCEPHALA LEAVES EXTRACT},
Journal = {INTERNATIONAL JOURNAL OF PHARMACEUTICAL SCIENCES AND RESEARCH},
Year = {2020},
Volume = {11},
Number = {12},
Pages = {6305-6310},
Month = {DEC},
Abstract = {The study aims to investigate the antioxidant and antidiabetic activity
   of Leucaena leucocephala ethanol leaf extract (LLME). L. leucocephala
   leaves extraction was carried out by the maceration method.
   Phytochemical analysis was performed using standard analytical
   procedures. Diphenyl picrylhydrazyl (DPPH) radical and 2, 2'-azinobis
   (3-ethylbenzathiazoline-6-sulfonic acid) (ABTS) radical scavenging
   assay; alpha-amylase and alpha-glucosidase inhibition assays were
   performed. The extraction yield of LLME by maceration was 65\%. The
   phytochemical screening confirmed the presence of alkaloids,
   monosaccharides, proteins, amino acids, steroids, flavonoids,
   glycosides, tannins, and saponins. The total phenol content of LLME was
   970 mg of GAE/g, and total flavonoid content was found to be 550 mg of
   QE/g. In DPPH and ABTS assay LLME showed good antioxidant activity
   (IC50: 11.21 +/- 0.2 and 11.87 +/- 0.05 mu g/mL) when compared to the
   standard dnig, ascorbic acid (IC50 :11.09 +/- 0.05 and 11.01 +/- 0.09 mu
   g/mL). Similarly, in alpha-ainvlase and alpha-glucosidase inhibition
   assay, acarbose revealed enzyme inhibition activity with an IC50 value
   of 12.02 +/- 0.02 and 12.11 +/- 0.02 mu g/mL while LLME showed an IC50
   value of 12.46 +/- 0.06 and 12.47 +/- 0.08 mu g/mL, respectively. The
   present study leads to the conclusion that L. leucocephala ethanol leaf
   extract is with tremendous antioxidant and antidiabetic properties.},
DOI = {10.13040/IJPSR.0975-8232.11(12).6305-10},
ISSN = {0975-8232},
ResearcherID-Numbers = {CHIGURUPATI, SRIDEVI/GRX-8274-2022},
ORCID-Numbers = {CHIGURUPATI, SRIDEVI/0000-0002-1081-2964},
Unique-ID = {WOS:000595532500045},
}

@article{ WOS:000180054400001,
Author = {Machín, LM and Valenciaga, N and Ruiz, TE and Mora, C and Noda, A},
Title = {Localization in time and space of Heteropsylla cubana (Crawford)
   in a Leucaena leucocephala (Lam) de Wit grassland},
Journal = {CUBAN JOURNAL OF AGRICULTURAL SCIENCE},
Year = {2002},
Volume = {36},
Number = {1},
Pages = {3-6},
Month = {MAR},
Abstract = {Leucaena leucocephala (Lam) de Wit is a legume used as forage of high
   nutritive value and acceptability. Those characteristics make it an
   alternative for animal feeding and production. It is well known that
   Heteropsylla cubana Crawfort (Hompotera:Psylidae) is the most
   significant pest affecting this legurne. This study was aimed at
   increasing the knowledge on the behavior and distribution of this
   psyllid in leucaena crops. Thus, samplings were carried out for two
   years in a leucaena grassland. Data were collected to calculate the
   spatial localization in each month. Ninety-one percent of the months
   were classified as contagion and the rest as regular. The annual
   temporal localization was also determined and classified as contagion
   with an agglomeration of isolated psyllids and a binomial negative
   probability distribution of data. Finally, the optimum sample size (1433
   trappings/year) was calculated under similar conditions.},
ISSN = {0864-0408},
ORCID-Numbers = {Valenciaga Valdes, Nurys/0000-0003-3514-2095},
Unique-ID = {WOS:000180054400001},
}

@article{ WOS:000449590100007,
Author = {Pramod, S. and Patel, Priti B. and Rao, Karumanchi S.},
Title = {Ethylene and its interaction with other hormones in tension wood
   formation in Leucaena leucocephala (Lam.) de Wit},
Journal = {JOURNAL OF THE INDIAN ACADEMY OF WOOD SCIENCE},
Year = {2012},
Volume = {9},
Number = {2},
Pages = {130-139},
Month = {DEC},
Abstract = {The effect of exogenous ethephon and its combination with gibberellic
   acid (GA3), indole-3-acetic acid (IAA) and benzyl amino purine (BAP) on
   tension wood formation in the vertically growing shoots of Leucaena
   leucocephala has been studied using light and scanning electron
   microscopy (SEM). The application of exogenous ethephon induced tension
   wood formation. The percentage of tension wood severity in the shoots
   was directly proportional to the concentration of ethephon applied. SEM
   studies revealed the replacement of S2 and S3 fibre wall layers with the
   gelatinous layer (G-layer) in response to high concentration of
   ethephon. Ethephon exhibited synergistic interaction with GA3 and BAP in
   tension wood formation. Fibres and vessel elements in tension wood,
   induced by ethylene and its combination with other hormones showed
   increase or decrease in their dimensions irrespective of their
   concentration and combination. The density of rays increased in all the
   treatments except in the combination of ethephon and IAA. The anatomical
   data indicate that ethylene plays a major role in tension wood formation
   and it interacts synergistically with gibberellins and cytokinin. On the
   other hand, such synergistic interaction was not found between ethephon
   and IAA in tension wood formation.},
DOI = {10.1007/s13196-012-0077-0},
ISSN = {0972-172X},
EISSN = {0976-8432},
ResearcherID-Numbers = {Sivan, Pramod/K-5386-2015},
ORCID-Numbers = {Sivan, Pramod/0000-0001-5297-2221},
Unique-ID = {WOS:000449590100007},
}

@article{ WOS:001355555600001,
Author = {Valencia-Olivares, C. and Franca-Oliveira, G. and Luna-Vital, D. A. and
   Hernandez-Ledesma, B.},
Title = {Green guaje (Leucaena leucocephala) and pigmented guaje (Leucaena
   esculenta) as sources of antioxidant and immunomodulatory peptides},
Journal = {FOOD CHEMISTRY},
Year = {2025},
Volume = {464},
Number = {3},
Month = {FEB 1},
Abstract = {In recent years, there has been a growing interest in plant-based diets,
   particularly legumes, as a sustainable and healthy dietary choice. This
   study breaks new ground by investigating the effects of simulated
   gastrointestinal digestion on green (Leucaena leucocephala) and
   pigmented (Leucaena esculenta) guaje proteins. We evaluated the
   antioxidant and immunomodulatory properties of ultrafiltered fractions
   resulting from digestion in a macrophage model. Both fractions showed
   promising potential as radical scavengers. The fraction <5 kDa from
   pigmented guaje, even at the lowest doses tested, significantly (p <
   0.05) inhibited the release of pro-inflammatory cytokines TNF-alpha and
   IL-6, and demonstrated an immunomodulatory effect by reducing the levels
   of ROS and NO. These findings suggest that green and pigmented guaje
   could be a valuable source of bioactive peptides, potentially used as a
   coadjutant for treating and preventing oxidative stress and
   inflammation-associated non-communicable diseases through the
   utilization of underutilized legumes.},
DOI = {10.1016/j.foodchem.2024.141781},
EarlyAccessDate = {NOV 2024},
Article-Number = {141781},
ISSN = {0308-8146},
EISSN = {1873-7072},
Unique-ID = {WOS:001355555600001},
}

@article{ WOS:A1991GZ28000003,
Author = {KARIM, AB and SAVILL, PS and RHODES, ER},
Title = {THE EFFECT OF YOUNG LEUCAENA-LEUCOCEPHALA (LAM) DEWIT HEDGES ON THE
   GROWTH AND YIELD OF MAIZE, SWEET-POTATO AND COWPEA IN AN AGROFORESTRY
   SYSTEM IN SIERRA-LEONE},
Journal = {AGROFORESTRY SYSTEMS},
Year = {1991},
Volume = {16},
Number = {3},
Pages = {203-211},
Abstract = {An investigation was undertaken at Senehun - Kamajei, a high rainfall
   region in Sierra Leone, to assess the effects of Leucaena leucocephala,
   on the growth and productivity of maize, cowpea and sweet potato.  The
   experiment was laid out in a completely randomised design with four
   replicates of each treatment.  The treatments and controls were:  (i)
   pure crops of maize, cowpea, sweet potato, Leucaena clean weeded and
   unweeded and (ii) intercrops of Leucaena with the food crops, both (iii)
   with and without applied fertilizers after the first year.
   The growth of Leucaena was slow but nevertheless tended to reduce grain
   yields of maize, and tuber and vine yields of sweet potato in the rows
   in the immediate vicinity of the trees, especially in the
   nitrogen-treated plots.  The maize, in particular, caused an improvement
   in the early height growth of the trees.},
DOI = {10.1007/BF00119317},
ISSN = {0167-4366},
Unique-ID = {WOS:A1991GZ28000003},
}

@article{ WOS:000426436300011,
Author = {Xu, Yingchao and Tao, Zhenru and Jin, Yu and Chen, Shuangyan and Zhou,
   Zhongyu and Gong, Amy G. W. and Yuan, Yunfei and Dong, Tina T. X. and
   Tsim, Karl W. K.},
Title = {Jasmonate-Elicited Stress Induces Metabolic Change in the Leaves of
   Leucaena leucocephala},
Journal = {MOLECULES},
Year = {2018},
Volume = {23},
Number = {2},
Month = {FEB},
Abstract = {The plant Leucaena leucocephala was exposed to four jasmonate elicitors,
   i.e., jasmonic acid (JA), methyl jasmonic acid (MeJA),
   jasmonoyl-L-isoleucine (JA-Ile) and 6-ethyl indanoyl glycine conjugate
   (2-{[}(6-ethyl-1-oxo-indane-4-carbonyl)-amino]-acetic acid methyl ester)
   (CGM). The treatment was to mimic the herbivores and wounding stresses.
   By using NMR spectroscopy along with chemometric analysis, including
   principal component analysis (PCA) and partial least squares
   discriminant analysis (PLS-DA), the changes of metabolites in the leaves
   of L. leucocephala were determined under the stress as induced by the
   four elicitors. The challenge of JA-Ile caused an accumulation of lactic
   acid (6), beta-glucose (10), alanine (12), threonine (13), steroids
   (18), 3,4-dihydroxypyridine (19) and an unidentified compound 20. The
   chemometric analysis of the PCA and PLS-DA models indicated that the
   alternation of metabolites triggered by JA, MeJA, and CGM treatments
   were very minimum. In contrast, the treatment by JA-Ile could induce the
   most significant metabolic changes in the leaves. Moreover, there was
   very minimal new metabolite being detected in responding to the
   jasmonate-induced stresses. The results showed some metabolite
   concentrations changed after application of the elicitors, which may be
   related to a high level of tolerance to stress conditions as well as the
   strong ecological suitability of L. leucocephala.},
DOI = {10.3390/molecules23020188},
Article-Number = {188},
EISSN = {1420-3049},
Unique-ID = {WOS:000426436300011},
}

@article{ WOS:000255990400011,
Author = {Haque, N. and Toppo, Saroj and Saraswat, M. L. and Khan, M. Y.},
Title = {Effect of feeding Leucaena leucocephala leaves and twigs on
   energy utilization by goats},
Journal = {ANIMAL FEED SCIENCE AND TECHNOLOGY},
Year = {2008},
Volume = {142},
Number = {3-4},
Pages = {330-338},
Month = {MAY 1},
Abstract = {Fifteen intact male goats of Jamunapari breeds, 12-15 months old
   weighing 22.0 kg were divided into three groups of five animals in each
   in a completely randomized design to determine the effect of feeding
   Leucaena leucocephala leaves and twigs (LLLT) on heat production (HP)
   and energy balance (EB). Animals in treatment I (Control) was supplied
   with a conventional ration consisting of a concentrate mixture and
   vegetative parts of Zea mays and in treatments II and III received
   similar rations except that LLLT provided about 0.40 and 0.50 parts
   crude protein (CP) of total protein in ration, respectively by replacing
   0.50 and 1.00 parts each of the solvent extracted groundnut cake and
   wheat bran in the concentrate mixture, respectively. The dry matter (DM)
   supplemented through LLLT was 0, 121 and 224 g in treatments I, II and
   III, respectively and it corresponds to 0, 0.19 and 0.34 parts of total
   DM intake. Supplementation of increasing amount of LLLT did not affect
   (P=0.303) DM intake but digestibility of DM decreased (P=0.002)
   linearly. The digestible energy (DE) intake was not affected (P=0.161)
   due to supplementation of LLLT. Urinary energy (UE) loss per kg
   metabolic body size per day and as proportion of gross energy (GE) or DE
   intake increased (P < 0.001) linearly with increased LLLT
   supplementation. Average methane production varied from 15.2 to 17.3
   L/d. Methane energy loss per kg metabolic body size per day decreased
   (P=0.024) linearly with increased supplementation of LLLT, however, as
   proportion of GE or DE did not show any effect of treatment.
   Metabolizable energy (ME) intake per kg metabolic body size per day was
   unaffected (P=0.113) by the treatments which corresponded to 1.63, 1.35
   and 1.38 times of maintenance in treatments I, II and III, respectively
   indicated that animals were in well-fed condition. Heat production (HP)
   and energy balance (EB) were not affected by the supplementation
   treatment. It is concluded that supplementation of 224 g of LLLT in the
   diet of goats, which provided DM at a proportion of 0.34 or CP at a
   proportion of 0.59, did not affect HP and EB. (c) 2007 Elsevier B.V. All
   rights reserved.},
DOI = {10.1016/j.anifeedsci.2007.09.027},
ISSN = {0377-8401},
ResearcherID-Numbers = {Khan, Muhammad Usman/AFA-9418-2022},
Unique-ID = {WOS:000255990400011},
}

@article{ WOS:000186479400013,
Author = {Febles, G and Torres, V and Ruiz, TE and Martínez, L and Díaz, H and
   Noda, A},
Title = {Use of the multivariate analysis to evaluate the production of seeds in
   accessions of Leucaena leucocephala in Cuba},
Journal = {CUBAN JOURNAL OF AGRICULTURAL SCIENCE},
Year = {2003},
Volume = {37},
Number = {3},
Pages = {299-305},
Month = {SEP},
Abstract = {A research was conducted, for two years. to evaluate the production of
   seeds in 90 accessions of Leucaena leucocephala from the Centro
   Internacional de Agricultura Tropical (CIAT) in Colombia. The
   multivariate analysis was used. The method of principal components
   showed that more than 90 \% of the variability, accumulated in three
   factors,,vas from the morphological indicators of yield. length and
   number of seeds/pods, weight of 1000 seeds and total weight of seed
   production/plant, in the analysis of the first and second year and in
   both. The method of conglomerates (Clusters) made possible the
   obtainment of groups that were reordered according to their biological
   performance and, at the same tulle, the accessions were grouped
   according to their similarities. There were groups of good performance
   in which the total yield of seed per plant was Lip to 300 g. while., in
   other groups, it was never Superior to 20 g. The number of seeds/pods
   was LIP to 24. The weight of 1000 seeds was. sometimes. 50.5 g, while
   the length of the pod varied from 10 to 22.8 cm. The cultivar Peru,
   widely used in Cuba. was among the best groups of accessions. The
   results showed that more than 70 \% of the accessions had good
   conditions to produce seeds in the western part Of the country. The best
   accessions and the necessity of their proper management are discussed.
   It is concluded that the multivariate analysis, strategically used. is
   all effective method to evaluate the production of seeds in large groups
   of accessions or plant species phylogenetically related. combining the
   mathematical results with the biological performance.},
ISSN = {0864-0408},
Unique-ID = {WOS:000186479400013},
}

@article{ WOS:000077308800019,
Author = {Sánchez, A},
Title = {Comparison between the spontaneous nodulation and its spatial
   distribution in two system of cultivation of Leucaena
   leucocephala (Lam) of wit},
Journal = {REVISTA CIENTIFICA-FACULTAD DE CIENCIAS VETERINARIAS},
Year = {1998},
Volume = {8},
Number = {1},
Pages = {66-68},
Month = {NOV},
Abstract = {In order to compare the spontaneous nodulation of Leucaena leucocephala
   with its spacial distribution, two system cultivation were analyzed:
   monocultivation and associated with grass (C. ciliaris). The experiment
   was conducted at the experimental campus La Canada, Fondo Nacional de
   Investigaciones Agropecuarias (FONAIAP) in La Canada de Urdaneta
   municipality, Zulia state, Venezuela, under very dry tropical forest
   conditions. Six plants of leucaena fdr each system were evaluated, and
   sampled at 25, 50 y 75 cm from the foot of the plants at two different
   depth 0-20 cm and 20-40 cm, in each side of the row. The number of
   nodule and its spatial distribution, weigth, height and total number of
   nodules per cubic meter were determined. The total randomiced statistic
   design was used. The obtained results show presence of nodule in both
   cultivation systems in the range of 1 to 2 mm of height. No
   significative difference was found (P>0.05) between number of nodules
   and the weigth of the nodules (9.3 vs 9.2 vs 0.02 g). On the other hand,
   the number of nodules was not affected by the horizontal and vertical
   distribution (P>0.05).},
ISSN = {0798-2259},
Unique-ID = {WOS:000077308800019},
}

@article{ WOS:000291140500023,
Author = {Nirmal, Nilesh Prakash and Benjakul, Soottawat},
Title = {Inhibition of melanosis formation in Pacific white shrimp by the extract
   of lead (Leucaena leucocephala) seed},
Journal = {FOOD CHEMISTRY},
Year = {2011},
Volume = {128},
Number = {2},
Pages = {427-432},
Month = {SEP 15},
Abstract = {Lead (Leucaena leucocephala) seed extract was prepared using distilled
   water as a medium. An extraction yield of 26.16 g/100 g of seed was
   obtained after extraction at room temperature for 12 h. Total phenolic
   and mimosine contents in the lead seed extract powder (LSEP) were 17.4g
   GAE/100 g and 8.8 g/100 g, respectively. LSEP at different
   concentrations (0.05\%, 0.1\%, 0.25\%, 0.5\%, and 1\%, w/v) showed
   inhibitory activity towards polyphenoloxidase (PPO) of Pacific white
   shrimp in a dose dependent manner. When the whole Pacific white shrimp
   were treated with 0.25\% and 0.5\% (w/v) LSEP, the shrimp treated with
   0.5\% LSEP had the lower melanosis score throughout the storage of 12
   days and showed a higher score for colour and odour, as well as overall
   likeness, compared with the control (without treatment) and 1.25\%
   sodium metabisulphite treated samples at day 12 (P < 0.05). Meat of
   shrimps treated with LSEP at both levels had the increase in mimosine
   content up to 8 days, suggesting the migration of mimosine into shrimp
   muscle during extended storage. Therefore, 0.5\% LSEP can be used as a
   novel melanosis inhibitor for Pacific white shrimp. (C) 2011 Elsevier
