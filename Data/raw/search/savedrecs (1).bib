@article{ WOS:000740649500020,
Author = {Amrullah, Apip and Farobie, Obie and Pramono, Gatut Pujo},
Title = {Solid degradation and its kinetics on phenol-rich bio-oil production
   from pyrolysis of coconut shell and Lamtoro wood residue},
Journal = {KOREAN JOURNAL OF CHEMICAL ENGINEERING},
Year = {2022},
Volume = {39},
Number = {2},
Pages = {389-397},
Month = {FEB},
Abstract = {The pyrolysis of coconut shell (CS) only, lamtoro (Leucaena
   leucocephala) wood residues (LWR) only, and a CS/LWR mixture was
   experimentally studied herein for the first time. Additionally, the
   reaction kinetics of solid destruction during the pyrolysis process of
   CS and LWR was deduced. An experimental investigation was carried out in
   a batch reactor at the different pyrolysis temperatures (300-500 degrees
   C). The highest phenol yield (30.97\%) was observed at 500 degrees C for
   the pyrolysis of CS. The activation energy and pre-exponential factor
   and for degradation of solid were successfully determined for the first
   time using the Arrhenius equation. The activation energy was determined
   in the range of 121-153 kJ mol(-1) for the temperature range of 300-500
   degrees C. Meanwhile, the pre-exponential factors of 3.51x10(10) s(-1),
   4.77x10(10) s(-1), and 5.38x10(10) s(-1) were calculated for the
   pyrolysis of CS only, LWR only, and a CS/LWR mixture, respectively. This
   research presents the mitigation for the alleviation of the energy
   crisis and to convert underutilized biomass to high-value products.},
