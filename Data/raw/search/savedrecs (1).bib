@inproceedings{ WOS:000674593000057,
Author = {Wiratmini, I, N. and Setyawati, I and Warmadewi, D. A. and Setiasih, N.
   L. E.},
Editor = {Nazir, N and Ito, N and Doni, F and Rahim, R and Hidayat, R},
Title = {Water-Soaked Leucaena Leucocephala Leaf Meal as Feed For Rabbits:
   Effect of Inclusion on the Performance},
Booktitle = {7TH INTERNATIONAL CONFERENCE ON SUSTAINABLE AGRICULTURE, FOOD AND ENERGY},
Series = {IOP Conference Series-Earth and Environmental Science},
Year = {2021},
Volume = {709},
Note = {7th International Conference on Sustainable Agriculture, Food and Energy
   (SAFE) - Green Agri-food Energy Production for a Better World in a
   Changing Climate, Phuket Rajabhat Univ, Phuket, THAILAND, OCT 18-21,
   2019},
Organization = {Chiang Mai Univ; Chiang Mai Rajabht Univ; Andalas Univ},
Abstract = {Leucaena leucocephala is high-nutrient legume due to its crude protein
   rate (20-30\%). Unfortunately, its utilization needs to be restricted
   due to the presence of mimosine, a compound that exhibits negative
   effects on livestocks. This study aimed to compare the performance of
   adult male rabbits after given commercial feed supplemented with
   water-soaked and air-dried Leucaenaleaf meal (WSADL) with those given
   air-dried Leucaena leaf meal (ADL).Fifty local male rabbits, 6 months
   old and weighing around 2.0-2.3 kg, were used as samples. This study
   used a Completely Randomized Design (CRD) with five treatment groups
   (n=10),consisting of one control group that were fed commercial feed
   (RO), three groups that were fed WSADL (15\%, 22.5\%, and 30\%) and one
   group that was fed 15\% of ADL. Those rabbits were fed for 60 days. The
   independent variables being observed were the dry matter intake, daily
   weight gain, and feed conversion ratio (FCR). The result showed that the
   supplementation of Leucaena leaf meal which was processed through
   water-soaking and air-dried as much as 30\% on rabbit feed did not
   reduce the dry matter intake, while it increased body weight gain and
   efficiency of feed usage.},
DOI = {10.1088/1755-1315/709/1/012058},
Article-Number = {012058},
ISSN = {1755-1307},
ResearcherID-Numbers = {Setyawati, Iriani/CAI-8851-2022
