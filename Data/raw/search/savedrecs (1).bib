@inproceedings{ WOS:000352343500130,
Author = {Bai, Ling and Chen, Fang-Qing},
Book-Group-Author = {Destech Publicat Inc},
Title = {Effects of Soil-fibrous Root System of Leucaena Leucocephala Community
   on Soil Conservation and Slope Protection in Shallow Soil Layer},
Booktitle = {INTERNATIONAL CONFERENCE ON ENERGY AND ENVIRONMENT ENGINEERING (ICEEE
   2015)},
Year = {2015},
Pages = {727-731},
Note = {International Conference on Energy and Environment Engineering (ICEEE),
   Nanjing, PEOPLES R CHINA, APR 11-12, 2015},
Abstract = {Leucaena leucocephala has been widely used in the ecological restoration
   and protection of engineering slopes in Xiluodu hydropower project. The
   erosion resistance, scour resistance, shear strength of soil-fibrous
   root systems and tensile strength of fibrous roots in shallow soil layer
   were tested to evaluate the effects of L. leucocephala communities on
   soil conservation and slope protection. The results showed that
   soil-fibrous root system of L. leucocephala community had significant
   effects on soil conservation and slope stabilization in shallow soil
   layer. The average erosion resistance, scour resistance and shear
   strength were increased by 66.2\%, 183.3\% and 217.0\% contrast with
   that of the control, respectively. The strengthening effects increased
   with the increase of community age. Correlation analysis indicated that
   the performance of soil-fibrous root systems had significant positive
   correlation with root system biomass. It suggested that the differences
   in root system biomass caused the changes of fibrous root systems in
   soil conservation and slope stabilization. Further analysis indicated
   that the performance of L. leucocephala-Buxus microphylla community was
   better than that of pure L. leucocephala community in same age in soil
   conservation and slope protection. Appropriate intermediate thinning and
   species introduction should by carry out in L. leucocephala community to
   improve the effects of soil-root system on soil conservation and slope
   protection.},
ISBN = {978-1-60595-218-5},
Unique-ID = {WOS:000352343500130},
}

