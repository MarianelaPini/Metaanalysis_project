@article{ WOS:000188214600007,
Author = {La, O and Chongo, B and Delgado, D and Valenciaga, D and Pratt, A and
   Elías, A and Ruiz, TE and Torres, V and Scull, I},
Title = {Characterization of in situ ruminal degradability of nutrients
   and in vitro intestinal digestibility of nitrogen in Leucaena
   leucocephala cv. CIAT-7929},
Journal = {CUBAN JOURNAL OF AGRICULTURAL SCIENCE},
Year = {2003},
Volume = {37},
Number = {4},
Pages = {381-387},
Month = {DEC},
Abstract = {Three bulls (Holstein x Zebu). eaullulated in the rumen with 415 +/- 22
   kg of liveweight (LW) were selected to study the in situ ruminal
   degradability of dry matter (DM), total nitrogen (Nt) nitrogen bound to
   neutral detergent fiber (N-NDF), nitrogen bound to acid detergent fiber
   (N-ADF) and the enzymatic digestibility. of the nitrogen non-degraded in
   the rumen in Leucaena leucocephala ev CIAT-7929. Bags were introduced in
   the rumen for 6, 8, 12, 24, 48 and 72 h. The degradability was analyzed
   by non-lineal regression with the use of the model P = a +
   b{*}(1-e(-c{*}(t-to))). The values of degradability of the insoluble but
   degradable, fraction were 470.3, 417.5, 543.5, 602.1 and 24.6 g/kg of
   DM, when time was not a limitation. The Lag time of the different
   fractions was approximately 0.40, 5.6, 15.2, 1.0, 93 and 18.5 h for DM,
   NDF, ADF, Nt, N-NDF and N-ADF, respectively. The effective degradability
   (ED) of Nt, N-NDF and N-ADF ranged between 579-514, 265-172 aud 0.8-0.4
   g/kg of DM, respectively with different constants of ruminal re-change.
   For all the indicators, the dynamics of in situ ruminal degradability
   showed increase (P < 0.01) of the exponential type in time, except for
   N-ADF, where degradation was almost insignificant. In vitro
   digestibility of nitrogen non-degraded in the rumen reached values of
   480 g of digestible nitrogen/kg of non-degraded nitrogen, with 24 h of
   permanence of the samples in the rumen. The increase of the ruminal
   incubation time up to 48 h, increased the digestibility of the nitrogen
   of the diet. Aspects on the use of the different components of the fiber
   fractions of this legume in ruminants are discussed. The results
   demonstrated a high contribution of nitrogen from leucaena in the rumen.
   Also a characterization of the nitrogen bound to the different fractions
   aud of its degradation in bulls rumen was obtained.},
ISSN = {0864-0408},
ORCID-Numbers = {Scull Rodriguez, Idania/0000-0002-9516-7182
   La O Leon, Orestes/0000-0001-8007-6053},
Unique-ID = {WOS:000188214600007},
}

@article{ WOS:000224572000021,
Author = {Mahmood, A and Iqbal, R and Qadri, R and Naz, S},
Title = {Some observations on mycorrhizae of Leucaena leucocephala (Lam.)
   De Wit.},
Journal = {PAKISTAN JOURNAL OF BOTANY},
Year = {2004},
Volume = {36},
Number = {3},
Pages = {659-662},
Month = {SEP},
Abstract = {In young roots of Leucaena leucocephala infection hyphae formed
   appresorium, sending branches in different directions to colonize the
   host cell. The mycorrhizal fungus was non-septate and branched. Fungal
   cells (Pelotons) were observed in the root cortex. Characteristic
   arbuscules commonly formed by VAM fungus were visible. The main hyphae
   branched and the branched hyphae terminated in an arbuscule. The
   cortical cells of the root showed large number of vesicles which were
   either globular or elliptical in shape.},
