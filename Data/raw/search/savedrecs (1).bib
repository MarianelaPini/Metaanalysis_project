@article{ WOS:A1988P961000007,
Author = {GUTTERIDGE, RC},
Title = {ALLEY CROPPING KENAF (HIBISCUS-CANNABINUS) WITH LEUCAENA
   (LEUCAENA-LEUCOCEPHALA) IN SOUTHEASTERN QUEENSLAND},
Journal = {AUSTRALIAN JOURNAL OF EXPERIMENTAL AGRICULTURE},
Year = {1988},
Volume = {28},
Number = {4},
Pages = {481-484},
Abstract = {Kenaf (Hibiscus cannabinus) was intercropped for 3 seasons between
   hedgerows of leucaena (Leucaena leucocephala) spaced 3 or 5 m apart on
   an infertile soil at Mt. Cotton, south-easten Queensland. The leucaena
   was cut at a height of 25 or 50 cm, 2 or 4 times per season and the
   fresh material spread as a mulch between the rows of kenaf. The yield of
   kenaf from mulched plots was compared with that from plots fertilised
   with nitrogen as urea at 0, 50 or 100 kg N/ ha.year. In the third
   season, kenaf yield from the mulched plots was maintained at about 4.7t
   DM/ha, whereas that from the control and urea fertilised plots declined
   by about 30\%. However, phosphorus deficiency in the unmulched plots was
   suspected and this may have contributed to the lower yield. Leucaena row
   spacing had no significant effect on kenaf yield. Leucaena yield ranged
   from 2 to 4t DM/ha per season. Cutting height had no significant effect
   on leucaena yield, but less frequent pruning gave a higher yield in the
   second and third seasons. The results confirm the potential of alley
   cropping to sustain arable crop yield at a moderate level without high
   inputs of inorganic fertilisers.},
DOI = {10.1071/EA9880481},
ISSN = {0816-1089},
EISSN = {1836-5787},
Unique-ID = {WOS:A1988P961000007},
}

@article{ WOS:A1996TW88500003,
Author = {Razz, R and Clavero, T and Ferrer, O and Rivero, Y and Amaya, A},
Title = {Energy content of two ecotypes of Leucaena leucocephala under different
   levels of fertilization},
Journal = {INTERCIENCIA},
Year = {1996},
Volume = {21},
Number = {1},
Pages = {21-\&},
Month = {JAN-FEB},
Abstract = {A field experiment was carried out in Zulia State, Venezuela, in order
   to evaluate the energy content of two ecotypes of Leucaena leucocephala;
   the zone is characterized as Tropical dry forest The evaluated factors
   were two ecotypes of Leucaena (CIAT 7984 and CIAT 17223), three levels
   of N (0, 30 and 60 kg/ha) and three levels of P (0, 75 and 150 Kg/ha).
   The treatments laid out in split-plot desing with three replications.
   The results showed that the ecotypes evaluated did not show
   significantly differences for energy content (EB), with mean values of
   4436 and 3948 cal/g MS for bulk and fine fractions, respectively. The
   different levels of N and P increased significantly (P<0.05) the content
   and yield of EB of the fine fraction. Gross energy content in the bulk
   fi action increased (P<0.05) with the application of 30 Kg N/ha, with
   3988 cal/g MS. The levels of EB in stems was affected (P<0.05) by the
   interaction N x P.},
ISSN = {0378-1844},
Unique-ID = {WOS:A1996TW88500003},
}

@article{ WOS:000186479400007,
Author = {La O, O and Chongo, B and Delgado, D and Ruiz, TE and Elías, A and
   Stuart, JR and Torres, V},
Title = {Dry matter and total nitrogen ruminal degradability of six ecotypes of
   Leucaena leucocephala},
Journal = {CUBAN JOURNAL OF AGRICULTURAL SCIENCE},
Year = {2003},
Volume = {37},
Number = {3},
Pages = {265-270},
Month = {SEP},
Abstract = {The variations in in situ ruminal dry matter (DM) and total nitrogen
   (TN) degradability were studied ill six ecotypes of Leucaena
   leucocephala (CIAT-7929,9379,9101,7872. 7988 and Peru), which are
