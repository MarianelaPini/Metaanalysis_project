Unique-ID = {WOS:000568915900001},
}

@inproceedings{ WOS:000445900200132,
Author = {Fattah, S. and Sobang, Y. U. L. and Samba, F. D. and Hartati, E. and
   Kapa, M. M. J. and Henuk, Y. L.},
