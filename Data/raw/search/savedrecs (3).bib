Unique-ID = {WOS:000305367800027},
}

@article{ WOS:000263410200009,
Author = {Das, D. K. and Chaturvedi, O. P. and Ali, M. S. and Kumar, R.},
Title = {Effect of leaf litters application on the growth and biomass production
   of Azadirachta indica A. Juss seedling},
Journal = {RANGE MANAGEMENT AND AGROFORESTRY},
Year = {2008},
Volume = {29},
Number = {2},
Pages = {115-118},
Month = {DEC},
Abstract = {The response of Azadirachta indica A. Juss seedlings to applied leaf
   litters from three sources viz. Leucaena leucocephala (Lam). de Wit,
   Sesbania grandiflora (L.) Poir and Dalbergia sissoo Roxb. at three
   levels each (20 gm, 40 gm and 60 gm kg(-1) of soil) was assessed in a
   completely randomized design with three replications. Each treatment
   consisted of 25 seedlings. The data on height and collar diameter and
   biomass yield indicated that leaf litter had significant effect on these
   parameters. The maximum seedling height (85.6 cm) and collar diameter
   (1.28 cm) was recorded with leaf litter applied at 60 gm kg(-1) of soil,
   which was statistically at par with 40 gm kg(-1). Highest plant height
   (93.0 cm) was recorded with S. grandiflora leaf litter applied @ 40 gm
   kg(-1) soil. However, maximum collar diameter was recorded with S.
   grandiflora leaf litter applied @ 60 gm kg(-1) soil. Total biomass yield
   varied from 24.93 gm in control to 53.84 gm in the plants supplemented
   with 60 gm of L. leucocephala. This study infers that S. grandiflora
   leaf application @.40 gm kg(-1) of soil is the optimum level for the
   plant growth whereas 60 gm L. lecucocephala leaf application is suitable
   for augmenting biomass yield.},
ISSN = {0971-2070},
ResearcherID-Numbers = {Ali, Mohammed/AGC-9214-2022},
Unique-ID = {WOS:000263410200009},
}

@article{ WOS:000253250900009,
Author = {Rodriguez, Alejandro and Perez, Antonio and de la Torre, Maria Jesus and
   Ramos, Enrique and Jimenez, Luis},
Title = {Neural fuzzy model applied to ethylene-glycol pulping of non-wood raw
   materials},
Journal = {BIORESOURCE TECHNOLOGY},
Year = {2008},
Volume = {99},
Number = {5},
Pages = {965-974},
Month = {MAR},
Abstract = {We studied the influence of the operational variables (viz.
   ethylene-glycol concentrations of 50-70\%, temperatures of 155-185 C,
   times of 30-90 min and numbers of PFI beating revolutions of 500-1500)
   on pulp yield and various paper properties (breaking length, stretch,
   burst index, tear index and brightness) obtained in the ethylene-glycol
   pulping of vine shoots, cotton stalks, leucaena (Leucaena leucocephala)
   and tagasaste (Chamaecytisus proliferus).
   The fuzzy neural network models used reproduced the experimental results
   with errors less than 15\% and smaller than those provided by
   second-order polynomial models in all cases.
   An ethylene-glycol concentration of 65\% at 180 C for 75 min and 1500
   PFI beating revolutions were found to provide substantial savings in
   energy, chemicals and facility investments as a result of operating
   under milder conditions than the strongest ones studied in this work.
   Tagasaste was found to be the most suitable raw material among those
   tested as it provided the paper sheets with the highest breaking length
   (4644 m), stretch (2.87\%), burst index (2.46 kN/g), tear index (0.33 in
   Nm(2)/g) and brightness (40.92\%); its pulp yield was also high
   (62.88\%), which reflects efficient use of this raw material. (c) 2007
   Elsevier Ltd. All rights reserved.},
DOI = {10.1016/j.biortech.2007.03.007},
ISSN = {0960-8524},
EISSN = {1873-2976},
ResearcherID-Numbers = {Ramos-Gómez, Enrique/L-2836-2014
