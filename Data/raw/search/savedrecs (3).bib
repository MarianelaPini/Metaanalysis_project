@inproceedings{ WOS:000074199600005,
Author = {Bino, BK},
Editor = {Daniel, JN and Roshetko, JM},
Title = {Biomass yield and nutrient turnover of six nitrogen fixing trees and
   shrubs in Papua New Guinea},
Booktitle = {NITROGEN FIXING TREES FOR FODDER PRODUCTION},
Series = {FOREST, FARM, AND COMMUNITY TREE RESEARCH REPORTS},
Year = {1998},
Number = {SI},
Pages = {40-45},
Note = {International Workshop on Nitrogen Fixing Trees for Fodder Production,
   PUNE, INDIA, MAR 20-25, 1995},
Organization = {Winrock Int, Forest Farm \& Community Tree Network; BAIF Dev Res Fdn},
Abstract = {Nitrogen fixing trees (NFTs) have the potential to contribute toward the
   sustainability of farming systems because they can provide wood for fuel
   and timber and fodder for animals; NFTs so sustain soil productivity
   through the addition of biomass as mulch and nitrogen fixation. In Papua
   New Guinea (PNG), the livelihood of about 75\% of the population depends
   on subsistence farming. With an annual population increase of 2.3\%, the
   traditional practice of shifting cultivation is unsustainable,
   especially in densely populated areas. As a result of the mandate given
   by the government of PNG to the Agriculture Research Division to develop
   sustainable farming systems based on low-input technology, studies to
   collect and evaluate various NFTs commenced in 1987. This paper reports
   on a study of six NFTs/shrubs in the highlands of Aiyura in PNG to
   identify suitable species for biomass production and use in cropping
   systems. Calliandra calothyrsus, Acacia angustissima, Leucaena
   diversifolia, and Leucaena mexicana produced more biomass than Leucaena
   leucocephala (cultivar K636) or Desmodium sequax, The four most
   productive species also contributed large amounts of major nutrients to
   the soil through hedge biomass whereas L. leucacephala (K636) and
   Desmodium sequax contributed the lowest. There was an increase in soil N
   and P levels and no change in soil K and organic carbon levels 2.5 years
   after planting NFTs.},
ISBN = {1-57360-011-3},
Unique-ID = {WOS:000074199600005},
}

@article{ WOS:000890915600005,
Author = {Barros Silva Leite, Maria Camila and Galvao dos Santos Freire, Maria
