@article{ WOS:000269580000005,
Author = {Plata, F. X. and Ebergeny, S. and Resendiz, J. L. and Villarreal, O. and
   Barcena, R. and Viccon, J. A. and Mendoza, G. D.},
Title = {Palatability and chemical composition of feeds ingested in captivity by
