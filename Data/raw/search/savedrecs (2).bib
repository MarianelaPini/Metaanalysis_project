Title = {L-SERINE O-SULFATE LYASE, A NEW ENZYME IN EXTRACTS FROM HIGHER-PLANTS},
Journal = {CHEMICAL \& PHARMACEUTICAL BULLETIN},
Year = {1977},
Volume = {25},
Number = {7},
Pages = {1829-1832},
Abstract = {L-Serine O-sulfate (L-SOS) lyase, an enzyme capable of degrading L-SOS
   to pyruvate, ammonia and sulfuric acid, was newly found in extracts from
   higher plants. The optimum pH for the L-SOS lyase in higher plants
   differ from that for the enzyme(s) in animals and microorganisms in the
   same buffer. The distribution and some properties of the L-SOS lyase in
   higher plants are described. {[}Plants studied were Phaseolus radiatus,
   Pisum sativum, Brassica campestris, Raphanus sativus, Fagopyrum
   esculentum, Mimosa pudica, Thermopsis chinensis, Lupinus luteus,
   Citrullus vulgaris, Leucaena leucocephala, Lathyrus odoratus, Mucuna
   capitata, Sophora flavescens, Robinia pseudoacacia, Thermopsis fabacea,
   Allium sativum and A. cepa.].},
ISSN = {0009-2363},
Unique-ID = {WOS:A1977DU42500046},
}

@article{ WOS:000233830800007,
Author = {Reinoso, M and Díaz, F and Simón, L},
Title = {Tree grasslands.: Nutritional benefits compared to monocultures},
Journal = {CUBAN JOURNAL OF AGRICULTURAL SCIENCE},
Year = {2005},
Volume = {39},
Number = {2},
Pages = {159-167},
Month = {JUN},
Abstract = {The effect of the inclusion of Leucaena leucocephala on the nutritional
   level and on the bioproductive perfon-nance of Siboney dairy cows was
   assessed. Three agroecosystems with trees of Leucaena leucocephala were
   used: (I) Cynodon nlemfuensis, planted on middle fertility soil, (II)
   natural pastures on poor fertility soils and (III) C nlemfuensis +
   Sporobolus indicus on middle fertility soils, as well as (IV), a control
   agroecosystem, based on the monoculture Panicum maximum on good
   fertility soil. All the herds consumed forage of king grass and
   sugarcane, according to the needs of feed balance, and different
   supplementation levels were used with molasses and concentrate. In all
   the agroecosystems with trees (I, II and III), the energy contribution
   of the ration was the main limiting factor to obtain superior milk
   yields, while in the monoculture agroecosystem (IV), it was the protein
   supply. In the dry season, the availability of nutrients from the
   endogenous feed sources of the agroecosystems I, II and III could
   guarantee between 3 and 5 kg of milk/cow/d, in total absence of external
   balanced feeds, while the IV showed higher dependence of the exogenous
   feed sources. In the agroecosystems I and III, more than 50 \% of the
   cows kept an ideal body condition throughout the lactation and more than
   65 \% arrived at calving with such condition, while in the II and IV,
   these values did not surpass 42 and 54 \%, respectively. It is concluded
   that in the agroecosystems in which leucaena was included in the
   grasslands, there are higher probabilities to attain a better nutrient
   balance in the ration of the animals. However, this depends on the
   requirements and nutritive quality of the basal pasture and the rest of
   the feeds in the diet.},
ISSN = {0864-0408},
EISSN = {2079-3480},
Unique-ID = {WOS:000233830800007},
}

@article{ WOS:000243627100018,
Author = {Shamseldin, Abdelaal and Werner, Dietrich},
Title = {Presence of Rhizobium etli bv. Phaseoli and
   Rhizobium gallicum bv. Gallicum in Egyptian soils},
Journal = {WORLD JOURNAL OF MICROBIOLOGY \& BIOTECHNOLOGY},
Year = {2007},
Volume = {23},
Number = {2},
Pages = {285-289},
Month = {FEB},
Abstract = {Seven bean rhizobial strains EBRI 2, 3, 21, 24, 26, 27 and 29 identified
   as Rhizobium etli, and EBRI 32 identified as Rhizobium gallicum,
   isolated from Egyptian soils and which nodulated Phaseolus vulgaris
   efficiently, were subjected to hybridization with a nifH probe in order
   to estimate the copy number of this gene. Seven strains (EBRI 2, 3, 21,
   24, 26, 27 and 29) which were only able to nodulate Phaseolus vulgaris,
   contained three copies of the nifH gene, consistent with their
   identification as Rhizobium etli bv. phaseoli. Only one strain (EBRI 32)
   which nodulated both Phaseolus vulgaris and Leucaena leucocephala, had
   one copy of nifH gene. This confirmed the classification of this strain
   as Rhizobium gallicum bv. gallicum.},
DOI = {10.1007/s11274-006-9204-7},
ISSN = {0959-3993},
Unique-ID = {WOS:000243627100018},
}

