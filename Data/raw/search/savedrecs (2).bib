@article{ WOS:A1985ALC9100010,
Author = {MALUF, AM and MARTINS, PS and MALUF, WR},
Title = {COTYLEDON PERSISTENCE IN THE SEEDLING AS A PARAMETER FOR EVALUATION OF
   ALUMINUM TOLERANCE IN LEUCAENA LEUCOCEPHALA},
Journal = {PESQUISA AGROPECUARIA BRASILEIRA},
Year = {1985},
Volume = {20},
Number = {3},
Pages = {355-360},
Abstract = {Cotyledon persistence in seedlings grown under Al stress, a good
   parameters for finding large differences in Al tolerance among L.
   leucocephala (Lam.) de Wit. introductions, was evaluated as a method to
   discriminate different levels of tolerance between and within tolerant
   populations. Twelve single plant progenies obtained from 3 tolerant
   populations, 1 intolerant introduction and 1 single-plant introduction
   of unknown tolerance were screened in nutrient solution under 9 ppm of
   Al.  Percentage of plants with yellowed or fallen cotylendons and total
   dry weight of the seedlings were recorded. The former parameter was
   efficient not only the discrimination of tolerant vs. intolerant
   populations, but also in the discrimination of different levels of
   tolerance within tolerant populations, contrasting in this respect with
   the latter parameter.},
