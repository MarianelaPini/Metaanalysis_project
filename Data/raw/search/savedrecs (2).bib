@article{ WOS:A1982NV68100003,
Author = {DMELLO, JPF and ACAMOVIC, T},
Title = {GROWTH-PERFORMANCE OF, AND MIMOSINE EXCRETION BY, YOUNG CHICKS FED ON
   LEUCAENA-LEUCOCEPHALA},
Journal = {ANIMAL FEED SCIENCE AND TECHNOLOGY},
Year = {1982},
Volume = {7},
Number = {3},
Pages = {247-255},
Abstract = {Growth performance and mimosine output in excreta were measured in young
   chicks fed on diets containing 3 forms of Leucaena leaf meal (LLM)
   imported from Malawi. In 1 of the experiments, the effects of
   supplementation of LLM with selected metal ions and enzymes were also
   measured. Growth and efficiency of food utilization were similar in
   groups fed on a control soybean meal diet or a basal diet containing 150
   g kg-1 of the whole leaf (WL) form of LLM. Supplementation of this basal
   LLM diet with FeSO4 or Al2(SO4)3 had little effect on growth or
   efficiency of food utilization, but the ratio of mimosine output to
   mimosine ingested (MO/MI) increased from 0.781-0.881 and 1.003 on
   addition of FeSO4 and Al2(SO4)3, respectively. Growth performance was
   unaffected by the addition of an enzyme mixture (containing cellulase,
   acid and neutral proteases and alpha amylase) to the basal LLM diet. In
   the 2nd experiment, 2 other forms of LLM were also tested: a ground leaf
   (GL) form, and a powder produced by grinding pellets of Leucaena leaf
   (GP). The WL and GP forms were each added at 50 and 100 g kg-1 diet and
   the GL form at 100 g kg-1 diet. These diets permitted daily intakes of
   mimosine ranging from 80-350 mg/replicate of 4 chicks. In comparison
   with control groups, growth performance was not impaired significantly
   in any of the LLM-fed groups. Mimosine excretion was high in all LLM-fed
   groups, the GP form inducing a maximum MO/MI ratio of 0.924.},
DOI = {10.1016/0377-8401(82)90018-9},
ISSN = {0377-8401},
EISSN = {1873-2216},
Unique-ID = {WOS:A1982NV68100003},
}

