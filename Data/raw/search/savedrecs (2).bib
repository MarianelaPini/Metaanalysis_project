Series = {FOREST, FARM, AND COMMUNITY TREE RESEARCH REPORTS},
Year = {1998},
Number = {SI},
Pages = {36-39},
Note = {International Workshop on Nitrogen Fixing Trees for Fodder Production,
   PUNE, INDIA, MAR 20-25, 1995},
Organization = {Winrock Int, Forest Farm \& Community Tree Network; BAIF Dev Res Fdn},
Abstract = {A series of field experiments, carried out at the Indian Grass land and
   Fodder Research Institute (IGFRI) in Jhansi, showed that Leucaena
   leucocephala can be successfully incorporated in forage forestry
   production systems with high-yielding perennial fodder crops. Among the
   fodder crops, hybrid napier was superior to guinea grass in forage
   production. Planting leucaena with hybrid napier variety IGFRI-3 in a
   paired row system was found to be the most appropriate crop arrangement.},
ISBN = {1-57360-011-3},
