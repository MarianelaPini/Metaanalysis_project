Journal = {CUBAN JOURNAL OF AGRICULTURAL SCIENCE},
Year = {1989},
Volume = {23},
Number = {2},
Pages = {151-156},
Month = {JUL},
Abstract = {Twenty-four 3/4 Z .times. 1/4 H growing-fattening yearlings with an
   initial LW of 162 kg were used to compare the animal performance of
   guinea grass with guinea + leucaena grasslands (30\% of the area) using
   a completely randomized design. The animals rotated in a 4 paddock
   grassland with 7 days of occupation per paddock. The stocking rate was
   2,3 animals/ha. The work was initiated when leucaena reached 1,20 m. On
   finishing the fattening period, 50\% of the animals grazing grass and
   legume were slaughtered for histopathological and hematological
   analyses. LW gain was 360 and 530 g/day for guinea grass and guinea +
   leucaena, respectively. The histopathological and hematological study
   showed that the animals grazing leucaena grass did not show health
   problems and the average weight values of thyroid were poorer than those
   found in the literature. LW gain in the grass-legume system makes it
   worthwhile with a savings of N fertilization and supplementation.},
ISSN = {0864-0408},
EISSN = {2079-3480},
Unique-ID = {WOS:A1989AZ82000004},
}

@article{ WOS:A1984SF48400001,
Author = {JONES, RM and JONES, RJ},
Title = {THE EFFECT OF LEUCAENA LEUCOCEPHALA ON LIVEWEIGHT GAIN, THYROID SIZE AND
   THYROXINE LEVELS OF STEERS IN SOUTHEASTERN QUEENSLAND},
Journal = {AUSTRALIAN JOURNAL OF EXPERIMENTAL AGRICULTURE},
Year = {1984},
Volume = {24},
Number = {124},
Pages = {4-9},
Abstract = {Animal and pasture production were measured from 1975 to 1980 on
   pastures based on L. leucocephala cv. Cunningham or cv. Peru, with
   Setaria sphacelata, at Samford, southeastern Queensland. Pastures were
   rotationaly grazed with stocking rates ranging throughout the year from
   1.3 beasts/ha (winter) to 4.) beasts/ha (summer). The cv. Cunningham
   pastures produced 427 kg liveweight gain/ha per yr, and the cv. Peru
   pastures 358 kg/ha per yr. The productivity of leucaena and its
   percentage in the diet of animals was consistently higher with cv.
   Cunningham than with cv. Peru. The average thyroid size of animals
   grazing year long was 50 and 147 g for cv. Peru and cv. Cunningham
   pastures (normal weights being .+-. 25 g) although thyroxine levels
   measured in early autumn were nerve suggestive of serious thryoxine
   deficiency. Thus the higher productivity and consumption of leucaena on
   cv. Cunningham pastures, due to a higher density of plants and greater
   productivity per plant, resulted in better animal production than was
   obtained from cv. Peru pastrues, despite the thyroid enlargement of
   animals grazing cv. Cunningham. Leucaena-based pastures have potential
   for commercial use in substropical eastern Australia.},
DOI = {10.1071/EA9840004},
ISSN = {0816-1089},
EISSN = {1836-5787},
Unique-ID = {WOS:A1984SF48400001},
}

@article{ WOS:A1979GS35800014,
Author = {FLORES, JF and STOBBS, TH and MINSON, DJ},
Title = {INFLUENCE OF THE LEGUME LEUCAENA-LEUCOCEPHALA AND FORMAL CASEIN ON THE
   PRODUCTION AND COMPOSITION OF MILK FROM GRAZING COWS},
Journal = {JOURNAL OF AGRICULTURAL SCIENCE},
Year = {1979},
Volume = {92},
Number = {APR},
Pages = {351-357},
Abstract = {Lactating Jersey cows (24) grazing a 3 wk regrowth of N fertilized
   Chloris gayana cv. Pioneer pastures selected a diet containing 18\%
   crude protein and were given 0, 2 or 4 kg/day of fresh L. leucocephala
   or 250 g/day of formal-casein. The effect of the treatments on milk
   production was measured in a 4 .times. 4 Latin-square design experiment
   with experimental periods of 14 days. Mean milk yield of unsupplemented
   cows was 9.6 kg/day, 10.1 kg/day when cows were supplemented with
   formal-casein and 10.3 kg/day when fed 2 or 4 kg Leucaena/day. Yields of
   solids-not-fat, protein, casein, butterfat and short and medium-chain
   (C4-C16) fatty acids in milk fat were higher when cows were fed
   supplements (P < 0.01). Intake of digestible organic matter was lowest
   on the unsupplemented diets although feed in excess of 40 kg D.M.{[}dry
   matter]/cow per day was offered and cows had little difficulty
   harvesting feed with a mean bite size of 350 mg organic matter/bite and
   mean grazing time of 507 min/24 h. A tropical grass diet containing 18\%
   crude protein was apparently deficient in protein for lactating cows and
   small quantities of the legume Leucaena can alleviate such a deficiency.},
DOI = {10.1017/S0021859600062870},
ISSN = {0021-8596},
EISSN = {1469-5146},
Unique-ID = {WOS:A1979GS35800014},
}

