@article{ WOS:A1968B025300017,
Author = {JOSHI, HS},
Title = {EFFECT OF FEEDING ON LEUCAENA LEUCOCEPHALA (LAM) DE WIT ON REPRODUCTION
   IN RATS},
Journal = {AUSTRALIAN JOURNAL OF AGRICULTURAL RESEARCH},
Year = {1968},
Volume = {19},
Number = {2},
Pages = {341+},
Abstract = {In separate experiments, mature rats were either given a ration
   containing L. leucocephala leaf meal at 15\% or 7.5\% level, or dosed
   orally each day with water extract of the leaf meal before and during
   the breeding period. At the 15\% level, the food intake of the rats was
   greatly reduced, and they showed general symptoms of inanition. All the
   females were infertile and the males showed reduced libido and
   fertility. In the other experiments the majority of the females
   conceived, but on autopsy it was observed that they had a significantly
   larger proportion of foetuses dead and undergoing resorption as compared
   with those of the control group. The results suggested that the toxic
   material in L. leucocephala, probably mimosine, produced infertility in
   the rat by reducing the food intake; when food consumption was adequate
   for reproductive function, the toxic material caused a high incidence of
   fetal death and resorption. {[}L. leucocephala is distributed throughout
   the tropics and has excellent forage qualities. Its use as an
   inexpensive protein supplement to animals has been limited because of
   the presence of a toxic water-soluble amino acid, mimosine,
   {[}beta]-{[}N-(3-hydroxypyridone-4)]-{[}alpha]-aminopropionic acid.].},
DOI = {10.1071/AR9680341},
ISSN = {0004-9409},
EISSN = {1836-5795},
Unique-ID = {WOS:A1968B025300017},
}

@article{ WOS:000229164900005,
Author = {Ajayi, B and Badejo, SOO},
Title = {Effects of board density on bending strength and internal bond of
   cement-bonded flakeboards},
Journal = {JOURNAL OF TROPICAL FOREST SCIENCE},
Year = {2005},
Volume = {17},
Number = {2},
Pages = {228-234},
Month = {APR},
Abstract = {Single-layered cement-bonded flakeboards were manufactured in the
   laboratory using two exotic tropical hardwood species, Gmelina arborea
   (G) and Leucaena leucocephala (L) and a combination of the two species
   at equal percentage (G + L). Boards were made to three densities of
   1000, 1100 and 1200 kg m(-3) and with homogenous particle size of 50 x
   2.5 mm in length and thickness respectively. The effects of board
   density on modulus of rupture (MOR) and internal bond (IB) of each
   species were investigated. MOR ranged from 7.7 to 9.8 N mm(-2) and 5.0
   to 6.4 N mm(-2) for boards made from G. arborea and L. leucocephala
   respectively. MOR for boards composed of equal quantities of both
   species (G + L) ranged from 5.5 to 8.9 N mm(-2). The IB values for G, L
   and G + L ranged from 0.11 to 0.16,0.06 to 0.08 and 0.08 to 0.12 N
   mm(-2) respectively. MOR was positively correlated with board density,
   unlike IB. Hence, boards from G. arborea, the lower density species,
   were stronger than those made from L. leucocephala or a combination of
   the two species. MOR of boards at different densities and species were
   significantly different, whereas there was no significant difference in
   the IB of boards at different densities.},
ISSN = {0128-1283},
ResearcherID-Numbers = {Ajayi, BO./HJP-2442-2023},
Unique-ID = {WOS:000229164900005},
}

@article{ WOS:000266973300006,
Author = {Diaz, A. and Martin, P. C. and Castillo, E. and Hernandez, J. L.},
Title = {Pre-fattening and fattening of Charolais males in grazing of tree
   legumes, silvopastoral system and biomass bank},
Journal = {CUBAN JOURNAL OF AGRICULTURAL SCIENCE},
Year = {2008},
Volume = {42},
Number = {2},
Pages = {151-154},
Month = {JUN},
Abstract = {Twelve Charolais Cuban males, of 140 kg of liveweight (LW) and 308 d of
   age were used for analyzing their productive performance. In the
   pre-fattening (June-September 2004), they grazed in a multiple
   association of legumes (Neonotonia wightii, Pueraria phaseloides and
   Macroptilium airopurpureum) and natural grassland. In the
   fattening-finalization (October 2004-June 2005) (with 241 kg of LW and
   445 of LW at slaughter) they grazed in an association of Leucaena
   leucocephala in 100 \% of the area and natural grassland; in the dry
   season, in 2 ha of biomass bank of Pennisetum purpureum vc. Cuba CT-115.
   The stocking rate in each subsystem associated was of 2 animals ha(-1)
   and 6 animals ha(-1) in the biomass bank. During the finalization (>=
   363 kg of LW), the animals received 2 kg of supplement (9.20 \% CP and
   10.46 MJ ME per kg of DM). The mean daily gain (MDG) was of 0.885 kg in
   the pre-fattening (113 d), of 0.550 kg during the 58 d in the biomass
   bank; of 0.878 kg in the period of 102 d in leucaena, in the dry season
   and of 0.911 kg during the 90 d of silvopastural finalization in the
   rainy season. The total permanence was of 363 d, with 0.838 kg of LW of
   MDG and 22 months of age at slaughter. High MDG of LW may be obtained
   with this livestock, in systems associated of legumes, shrubs and
   natural grassland, reducing the MDG of LW in the biomass bank during the
   dry season without fertilization or irrigation.},
ISSN = {0864-0408},
EISSN = {2079-3480},
ResearcherID-Numbers = {Hernandez, Jose/H-9562-2019},
Unique-ID = {WOS:000266973300006},
}

@article{ WOS:000280481000039,
Author = {Dias, Luiz Eduardo and Melo, Roseli Freire and Vargas de Mello, Jaime
   Wilson and Oliveira, Juraci Alves and Daniels, Walter Lee},
Title = {GROWTH OF SEEDLINGS OF PIGEON PEA (Cajanus cajan (L.) MILLSP),
   WAND RIVERHEMP (Sesbania virgata (CAV.) PERS.), AND LEAD TREE
   (Leucaena leucocephala (LAM.) DE WIT) IN AN ARSENIC-CONTAMINATED
   SOIL},
Journal = {REVISTA BRASILEIRA DE CIENCIA DO SOLO},
Year = {2010},
Volume = {34},
Number = {3},
Pages = {975-983},
Month = {MAY-JUN},
Abstract = {Phytoremediation strategies utilize plants to decontaminate or
   immobilize soil pollutants. Among soil pollutants, metalloid As is
   considered a primary concern as a toxic element to organisms. Arsenic
   concentrations in the soil result from anthropogenic activities such as:
   the use of pesticides (herbicides and fungicides); some fertilizers; Au,
   Pb, Cu and Ni mining; Fe and steel production; coal combustion; and as a
   bi-product during natural gas extraction. This study evaluated the
   potential of pigeon pea (Cajanus cajan), wand riverhemp (Sesbania
   virgata), and lead tree (Leucaena leucocephala) as phytoremediators of
   soils polluted by As. Soil samples were placed in plastic pots,
   incubated with different As doses (0; 50; 100 and 200 mg dm(-3)) and
   then sown with seeds of the three species. Thirty (pigeon pea) and 90
   days after sowing, the plants were evaluated for height, collar diameter
   and dry matter of young, intermediate and basal leaves, stems and roots.
   Arsenic concentration was determined in different aged leaves, stems and
   roots to establish the translocation index (TI) between the plant root
   system and aerial plant components and the bioconcentration factors
   (BF). The evaluated species showed distinct characteristics regarding As
   tolerance, since the lead tree and wand riverhemp were significantly
   more tolerant than pigeon pea. The high As levels found in wand
   riverhemp roots suggest the existence of an efficient accumulation and
   compartmentalization mechanism in order to reduce As translocation to
   shoot tissues. Pigeon pea is a sensitive species and could serve as a
   potential bioindicator plant, whereas the other two species have
   potential for phytoremediation programs in As polluted areas. However,
   further studies are needed with longer exposure times in actual field
   conditions to reach definite conclusions on relative phytoremediation
   potentials.},
DOI = {10.1590/S0100-06832010000300039},
ISSN = {0100-0683},
ORCID-Numbers = {Oliveira, Juraci/0000-0003-0150-2291},
Unique-ID = {WOS:000280481000039},
}

@inproceedings{ WOS:000387732600012,
Author = {Rosida, Dedin Finatsiyatull and Hapsari, Nur and Hidayah, Taufik},
Editor = {Jamari, J and Handogo, R and Suryani, E},
Title = {FUNCTIONAL PROPERTIES OF LEUCAENA LEUCOCEPHALA PROTEIN CONCENTRATES
   RESULTED SEPARATION OF ULTRAFILTRATION MEMBRANE},
Booktitle = {3RD BALI INTERNATIONAL SEMINAR ON SCIENCE \& TECHNOLOGY (BISSTECH 2015)},
Series = {MATEC Web of Conferences},
Year = {2016},
Volume = {58},
Note = {3rd Bali International Seminar on Science \& Technology (BISSTECH),
   Bali, INDONESIA, OCT 15-17, 2015},
Abstract = {Protein is a polypeptide, which is composed of a series of amino acids,
   with a relatively very large molecular weight, which ranges from 8,000
   to 10,000 Da. Separation of protein can be done by chemical, enzymatic
   or using an ultrafiltration membrane. Applications of protein
   concentrates product depends on its functional properties such as water
   absorption, oil absorption capacity and power emulsions, and foaming
   power which will affect the quality of product. Food product that use
   protein concentrates in the form of sausages, meatballs, bakery and
   beverages. Techniques of membrane separation are generally based on
   particle size and molecular weight of the thrust in the form of pressure
   differences, and different electric field concentration. This study aims
   to determine the role of membrane ultrafiltration to separate proteins
   and produce the characteristic functional properties. This study used
   two main factors, ultrafiltration membrane pressure (2.33 atm, 3.66 atm)
   and the separation time (15 minutes, 30 minutes, 45 minutes). The
   resulted showed leucaena seed protein concentrates obtained from the
   best treatment of 3.66 atm pressure and separation time 45 minutes.
   Concentrates of leucaena seed had the functional properties of the water
   absorption capacity of 3.3 g water/g of solid, oil absorption capacity
   of 1.43 g of oil/g solid, Bulk density of 0.55 g/ml, power emulsion
   51.11\% and power foam 9.8\%.},
DOI = {10.1051/matecconf/20165801012},
Article-Number = {01012},
ISSN = {2261-236X},
ResearcherID-Numbers = {finatsiyatull rosida, dedin/GPX-2494-2022
