@article{ WOS:A1985ARK7000022,
Author = {AKBAR, MA and GUPTA, PC},
Title = {PROXIMATE COMPOSITION, AND TANNIN AND MINERAL CONTENTS OF DIFFERENT
   CULTIVARS AND OF VARIOUS PLANT-PARTS OF SUBABUL (LEUCAENA-LEUCOCEPHALA)},
Journal = {INDIAN JOURNAL OF ANIMAL SCIENCES},
Year = {1985},
Volume = {55},
Number = {9},
Pages = {808-812},
Abstract = {Twelve cultivars of subabul were analysed for proximate composition,
   tannin and mineral contents. The various parts of the plant (''K-8'')
   were also analysed for proximate composition and minerals. The CP
   contents of these subabul cultivars ranged from 19 to 26\%.
   `'Cunningham'' and `'Peru'' varieties had the highest CP content. Among
   the plant parts seed and immature leaves contained highest amount of CP;
   stem and dry pods had the lowest. The tannin content of different
   cultivars was 7.33 (''K-8'') to 19.00 mg/g (''Dehra Dun''). All the
   varieties were rich in macro- and microminerals except sodium. The Ca to
   P ratio was quite wide. Being rich in protein subabul fodder may be used
   as a source of protein in the rations of dairy animals.},
ISSN = {0367-8318},
Unique-ID = {WOS:A1985ARK7000022},
}

@article{ WOS:A1981ND33600006,
Author = {SANZONOWICZ, C and COUTO, W},
Title = {EFFECT OF CALCIUM, SULFUR AND OTHER NUTRIENTS ON DRY-MATTER YIELD AND
   NODULATION OF LEUCAENA LEUCOCEPHALA IN A CERRADO SOIL},
Journal = {PESQUISA AGROPECUARIA BRASILEIRA},
Year = {1981},
Volume = {16},
Number = {6},
Pages = {789-794},
Abstract = {The response of L. leucocephala Lam de Wit. cv. Cunningham) to several
   nutrients was tested in a Dark Red Latosol (Haplustox) in the
   greenhouse. Levels used were 0 and 500 ppm of CaCO3, 0 and 40 ppm of K,
   0 and 15 ppm of S, 0 and 10.2 ppm of Mg, 0 and 1 ppm of Cu, 0 and 1 ppm
   of Zn, 0 and 0.5 ppm of B, 0 and 0.25 ppm of Na2MoO4.cntdot.2H2O and a
   basic application of 50 ppm of P. The highest increases in dry matter
   yield, root weight and number and total weight of root nodules
   (Rhizobium) were observed when Ca and S were applied. An interaction
   between Ca .times. S was also observed. Cu increased dry matter yield
   only in the absence of K. The application of CaCO3 increased Ca content
   and reduced Mg content in plants but total Ca and Mg uptake were
   increased. Calcium carbonate increased soil pH from 4.3 to 5.0 while
   exchangeable Ca increased from 0.8 to 1.4 meq/100 ml of soil.
   Exchangeable Mg and Al decreased from 0.6 to 0.4 meq/100 ml of soil and
   from 0.81 to 0.47 meq/100 ml of soil, respectively. S application
   increased Ca and Mg uptake. K, Mg, Zn, Mo and B did not affect either
   dry matter yield or number and weight of nodules. Cu increased root
   weight only.},
ISSN = {0100-204X},
EISSN = {1678-3921},
Unique-ID = {WOS:A1981ND33600006},
}

@article{ WOS:A1983SF81400018,
Author = {MEGARRITY, RG and JONES, RJ},
Title = {TOXICITY OF LEUCAENA-LEUCOCEPHALA IN RUMINANTS - THE EFFECT OF
   SUPPLEMENTAL THYROXINE ON GOATS FED ON A SOLE DIET OF LEUCAENA},
Journal = {AUSTRALIAN JOURNAL OF AGRICULTURAL RESEARCH},
Year = {1983},
Volume = {34},
Number = {6},
Pages = {791-798},
Abstract = {Daily injections of thyroxine were ineffective in overcoming the toxic
   effects of feeding Leucaena to goats. The goats did not differ from the
   controls in live weight change or body condition over the 15 wk of
   feeding Leucaena and both groups of animals developed esophageal
   lesions. Treated animals maintained normal serum thyroxine (T4) and
   triiodothyronine (T3) levels, and did not exhibit the thyroid
   hyperplasia of the controls. An increase in the serum T4 and T3 levels
   of 3 controls after 5 wk of feeding was associated with declining
   mimosine concentration in the feed, and the ability of the hyperplastic
   thyroids to produce sufficient T4. Serum analyses suggested Zn
   deficiency in the controls, associated with their low thyroid status.
   The goitrogenicity of 3-hydroxy-4(1H)-pyridone (DHP) was only partly
   responsible for the toxicity of Leucaena to ruminants, and the low feed
   intakes and low live weight gains were related to other effects of DHP.
   The evidence of extra-ruminal metabolism of DHP was discussed.},
DOI = {10.1071/AR9830791},
ISSN = {0004-9409},
EISSN = {1836-5795},
Unique-ID = {WOS:A1983SF81400018},
}

@article{ WOS:000288717200022,
Author = {Jetana, T. and Vongpipatana, C. and Usawang, S. and Sophon, S.},
Title = {Using Treated Leucaena (Leucaena leucocephala) Leaves as
   Supplements to Thai Brahman Cattle Giving a Basal Diet of Rice Straw},
Journal = {JOURNAL OF ANIMAL AND VETERINARY ADVANCES},
Year = {2011},
Volume = {10},
Number = {8},
Pages = {1054-1060},
Abstract = {Four male Thai Brahman cattle (261 +/- 3.2 kg; 24-36 months old) were
   fed rice straw as a basal diet and Leucaena treated in one of three
   methods: untreated (control), Leucaena treated with NaOH solution,
   Leucaena+PEG (Poly Ethylene Glycol), Using Leucaena treated with NaOH
   solution as a supplement improved (p<0.05) fibre digestion but N-balance
   reduced (p<0.05). Whilst N-balance, N-digestibility and fibre increased
   (p<0.05) but Dry Matter (DM) digestibility decreased (p<0.05) when
   animals supplemented with Leucaena+PEG. The effect of Condensed Tannins
   (CT) in Leucaena depends on the treatment of Leucaena, NaOH solution and
   PEG. Using NaOH solution treatment is potentially beneficial for
   improving fibre digestion but N-balance was reduced. However, adding PEG
   beneficial for improving fibre digestion, N-balance and N-digestion but
   DM digestion decreased.},
ISSN = {1680-5593},
Unique-ID = {WOS:000288717200022},
}

@article{ WOS:000266973300015,
Author = {Herrera, Madelen and Valenciaga, Nurys and Cortes, B. and Assis, C. and
   de Calzadilla, Josefina and Mora, C.},
Title = {Space disposition of the nests of Atta insularis Guerin in a
   grassland with predominance of Leucaena leucocephala and
   Panicum maximum},
Journal = {CUBAN JOURNAL OF AGRICULTURAL SCIENCE},
Year = {2008},
Volume = {42},
Number = {2},
Pages = {193-197},
Month = {JUN},
Abstract = {The space disposition of the nests of cut ants (Atta insularis) was
   studied in a silvopastoral system of Lecucaena leucocephala and Panicum
   maximum. The leucaena plants were sown between 1995 and 1997. The
   samplings were made in nine paddocks of this system between January and
   July in 2007. In the grassland, there were subtypes of typical Red
   Ferrallitic soil, the oxisol and hydrated or the ultisol. The paddocks
   were subdivided in 56 paddocks of 100 m(2). In these subdivisions, the
   number and distribution of the insect nests, the leucaena plants, and
   other shrubs were recorded. The variance/mean and Taylor indices were
   used to know the type of distribution of the nests. The factorial
   multivariated analysis was applied by multiple correspondences, as well
   as the Mann Whitney and Friedman tests to determine the effect of the
   soil, the trees and the paddocks in the leaf-cutting ants nests. The
   space distribution was of the aggregated type (ID=6.25, a=3.79 and
   b=1.42) with 99 nests ha(-1). The highest concurrence of nests (125
   nests ha(-1), P < 0.05) of leucaena (679 plants ha(-1), P < 0.001) and
   other trees (22 plants ha(-1), P < 0.05) took place in the hydrated red
   ferrallitic soil. The paddocks with this soil, sown in 1996, coincided
   with being the highest number of nests and of plants per hectare. It is
   concluded that the nests of A. insularis showed an aggregated space
   distribution. The soil and the tree plants affected the space
   distribution of the bibijagua nests in the leucaena-Guinea grass
   silvopastoral system.},
ISSN = {0864-0408},
EISSN = {2079-3480},
Unique-ID = {WOS:000266973300015},
}

@article{ WOS:A1992HM31800003,
Author = {MATHEW, T and KUMAR, BM and BABU, KVS and UMAMAHESWARAN, K},
Title = {COMPARATIVE PERFORMANCE OF 4 MULTIPURPOSE TREES ASSOCIATED WITH 4 GRASS
   SPECIES IN THE HUMID REGIONS OF SOUTHERN INDIA},
Journal = {AGROFORESTRY SYSTEMS},
Year = {1992},
Volume = {17},
Number = {3},
Pages = {205-218},
Abstract = {The results of a field trial conducted at the Livestock Research
   Station, Thiruvazhamkunnu, Kerala to study the compatibility of
   different components in a silvo-pastoral system revealed that growth and
   yield of fodder species were significantly influenced by the tree
   components only after tree canopy formation. The fodder species such as
   Pennisetum purpureum Schum., Panicum maximum Jacq., Brachiaria
   ruziziensis Griseb. and Euchlaena mexicana Schrad. grown in association
   with Casuarina equisetifolia J. R. \& G. Forst. and Ailanthus malabarica
   DC recorded comparatively higher forage yield even after canopy
   formation. However, forage crops grown in combination with Acacia
   auriculiformis A. Cunn. ex Benth. and Leucaena leucocephala (Lam.) de
   Wit. registered relatively lower values for growth and yield. Increased
   light infiltration into the understorey due to the cladophyllous canopy
   can be attributed as reason for the higher fodder productivity under
   Casuarina. Due to the combination of crown size and shape, tree height
   and spacing the amount of light intercepted by Ailanthus also was very
   low. Among the four multi-purpose trees used, Acacia recorded the
   maximum growth rate followed by Casuarina, Ailanthus and Leucaena.
   Forage productivity of the four species was in the order: Pennisetum
   purpureum > Panicum maximum > Brachiaria ruziziensis > Euchlaena
   mexicana. Casuarina with Pennisetum/Panicm were found to be optimal tree
   - forage combinations for silvo-pastoral systems.},
DOI = {10.1007/BF00054148},
ISSN = {0167-4366},
ResearcherID-Numbers = {Kumar, B./H-8002-2019},
Unique-ID = {WOS:A1992HM31800003},
}

