   Lopes, Guilherme/0000-0002-7898-798X
   Sousa Soares, Claudio Roberto Fonseca/0000-0002-5443-6614
   de Souza Moreira, Fatima Maria/0000-0003-0159-5811},
Unique-ID = {WOS:000325642800008},
}

@article{ WOS:A1979HG13600017,
Author = {TEITEI, T and HARRIS, RLN},
Title = {TROPOLONE ANALOGS OF THE AMINO-ACID MIMOSINE},
Journal = {AUSTRALIAN JOURNAL OF CHEMISTRY},
Year = {1979},
Volume = {32},
Number = {6},
Pages = {1329-1337},
Abstract = {The synthesis of
   DL-.alpha.-amino-.beta.-(4-hydroxy-5-oxocyclohepta-1,3,6-trienyl)propano
   ic acid and the corresponding 6-hydroxy isomer, tropolone analogs of the
   wool-growth inhibiting amino acid mimosine {[}Leucaena leucocephala], is
   described. Both isomers were obtained in a single sequence, commencing
   with the cycloaddition of dichloroketene to
   (.beta.,.beta.-diethoxyethyl)cyclopentadiene and were readily separated
   at an intermediate stage via the Cu complexes. These compounds may be
   applicable as chemical defleecing agents for sheep.},
