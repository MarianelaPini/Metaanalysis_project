@article{ WOS:A1979GS32100003,
Author = {HEGARTY, MP and LEE, CP and CHRISTIE, GS and COURT, RD and HAYDOCK, KP},
Title = {GOITEROGEN 3-HYDROXY-4(1H)-PYRIDONE, A RUMINAL METABOLITE FROM
   LEUCAENA-LEUCOCEPHALA - EFFECTS IN MICE AND RATS},
Journal = {AUSTRALIAN JOURNAL OF BIOLOGICAL SCIENCES},
Year = {1979},
Volume = {32},
Number = {1},
Pages = {27-40},
Abstract = {Mice fed a diet containing 1\% (wt/wt) 3-hydroxy-4(1H)-pyridone (DHP)
   developed goiter even with a diet high in I, whereas mimosine (0.5\%
   wt/wt) did not produce goiter even with a low-I diet. Thyroid
   enlargement was apparent (measured morphometrically) by the 7th wk and
   was advanced by the 11th wk. Histologically the goiter was hyperplastic.
   No marked histological changes were found in other organs of mice fed
   DHP or any organs of mice fed mimosine, except for some atrophy of hair
   follicles. A single intragastric dose of DHP inhibited the uptake of
   125I by the thyroid in the rat but an equivalent dose of mimosine did
   not. The inhibition apparently occurs at the I binding step, as with
   methyl thiouracil, rather than at the I trapping step, as with
   thiocyanate. Chronic treatment of mice with DHP, as with 6-methyl
   thiouracil, increased the avidity of the thyroid in taking up 125I. The
   major conjugated form of DHP in mammals, DHP-3-O-glucuronide, was almost
   as effective a goitrogen as the unconjugated compound when given orally,
   but considerably less active than the free form in the blood stream. DHP
   is a potent antithyroid compound of the thiouracil type with low general
   toxicity, since mammals can tolerate a level of intake sufficient to
   produce goiter in spite of I supplementation. {[}This study is relevant
   to feed toxicity to cattle and sheep.].},
DOI = {10.1071/BI9790027},
ISSN = {0004-9417},
Unique-ID = {WOS:A1979GS32100003},
}

