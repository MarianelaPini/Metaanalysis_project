@article{ WOS:A1989AB92200026,
Author = {KISHORE, N and VIRK, AS and YADAV, IS and SAGAR, V},
Title = {EFFECT OF FEEDING SUBABUL HAY (LEUCAENA-LEUCOCEPHALA) ON THE MILK-YIELD
   AND ITS COMPOSITION IN GOATS},
Journal = {INDIAN JOURNAL OF ANIMAL SCIENCES},
Year = {1989},
Volume = {59},
Number = {6},
Pages = {722-725},
Month = {JUN},
Abstract = {The concentrate mixture (T1) was replaced at 50 (T2) and 100\%(T2)
   levels with subabul for the formulation of 3 isonitrogenous feeds. Gram
   straw was offered ad lib. to meet deficit energy to 18 milk goats. The
   daily DM intakes were 1.64, 1.62 and 1.51 and and 1.41, 1.46 and 1.47 kg
   in respective 3 treatments during the 2 trials. Complete replacement of
   concentrate mixture leucaena in the early lactation of goats caused
   significant decline (P < 0.01) in the digestibility of all the nutrients
   except crude fiber and reduced milk yield. Milk-fat, protein, total
   solids and SNF contents did not differ significantly amongst treatments
   in both the trials. The findings suggested economical raising of
   lactating goats in their last stage of lactation on leucaena, as protein
   source, and gram straw. However, for goats in early lactation lower than
   50\% replacement of concentrate mixture with leucaena may be studied.},
ISSN = {0367-8318},
