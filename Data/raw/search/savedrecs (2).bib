@article{ WOS:A1968A980100010,
Author = {PRITCHARD, AJ and COURT, RD},
Title = {CYTOLOGICAL EFFECTS OF MIMOSINE},
Journal = {CYTOLOGIA},
Year = {1968},
Volume = {33},
Number = {1},
Pages = {73+},
Abstract = {Treatment of dividing cells in root tips of Vicia faba with either
   mimosine or the sodium salt of mimosine in aqueous solution resulted in
   clumping and non-division of chromosomes at metaphase. A c-mitosis
   sometimes resulted from treatment with relatively low concentrations.
   Clumped nuclei did not divide while the mimosine treatment was
   maintained but growth was possible in radicles of V. faba after a
   treatment of 24 hr. in a 1\% solution of the sodium salt. Mimosine also
   caused chromosome clumping in the root tips of Glycine javanica, Secale
   cereale, Allium cepa, Hordeum vulgare and Leucaena leucocephala.},
DOI = {10.1508/cytologia.33.73},
ISSN = {0011-4545},
Unique-ID = {WOS:A1968A980100010},
}

@inproceedings{ WOS:000282798600008,
Author = {Upadhyay, A. and Uezato, Y. and Tawata, S. and Ohkawa, H.},
Editor = {Shoun, H and Ohkawa, H},
Title = {CYP2C9 Catalyzed Bioconversion of Secondary Metabolites of Three
   Okinawan Plants},
Booktitle = {16TH INTERNATIONAL CONFERENCE ON CYTOCHROME P450, PROCEEDINGS},
Year = {2009},
Pages = {31+},
Note = {16th International Conference on Cytochrome P450 - Biochemistry,
   Biophysics, Biotechnology, Nago, JAPAN, JUN 21-25, 2009},
Abstract = {Bioconversions of secondary metabolites 5,6-dehydrokawain (DK), mimosine
   and phenolic acids of three Okinawan plants viz. alpinia (Alpinia
   zerumbet), leucaena (Leucaena leucocephala) and pineapple (Ananus
   comosus) using CYP2C9 isozyme and porcine microcosomal protein were
   studied. These plants are popular in Okinawa, and since these
   metabolites have their own pharmacological significances, this work was
   aimed at identifying possible metabolites using cytochrome enzyme
   systems. DK was metabolized to hispidin, while mimosine was degraded to
   some unknown compounds using CYP2C9 isozyme and porcine mircosomal
   protein. The mode of metabolism of these metabolites seems to be
   interesting, yet could not be defined well.},
ISBN = {978-88-7587-524-4},
Unique-ID = {WOS:000282798600008},
}

@article{ WOS:000943016900015,
Author = {Ysac Avila-Serrano, Narciso and Jacobo Lopez-Garrido, Serafin and
   Marcela Galicia-Jimenez, Monica and De Jesus Gonzalez-Crespo, Guillermo
   and Antonio Camacho-Escobar, Marco},
Title = {Effect of incorporating arboreal vegetation to Cynodon
   nlemfuensis diets during in-vitro ruminal fermentation},
Journal = {TERRA LATINOAMERICANA},
Year = {2020},
Volume = {38},
Number = {2},
Pages = {403-412},
Month = {APR-JUN},
Abstract = {Gramineae do not meet the needs of ruminants in the tropics, and their
   marked seasonal production causes disadvantages in animal production; in
   addition, high fiber levels and low protein content result in a higher
   production of greenhouse gases (GHG). For this reason, this study
   intended to supplement the diet by adding different foliage levels of
   tropical trees with the objective of evaluating the effect of
   incorporating 15, 30, and 45\% of Moringa oleifera, Leucaena
   leucocephala or Guazuma ulmifolia on a diet based on Cynodon nlemfuensis
   grass during ruminal fermentation in vitro. The methodology consisted of
   ten treatments with different inclusion percentages of three different
   trees; the chemical composition, in vitro ruminal fermentation, dry
   matter degradation (IVDMD), pH, volatile fatty acid concentration
   (VFAs), biogas production and emission of methane (CH4) and carbon
   dioxide (CO2) were determined. Data were analyzed through a completely
   randomized design. The chemical composition of the diets showed
   differences (P < 0.05) when tree foliage was added gradually to the
   diet; the IVDMD was different (P < 0.05); pH of the treatments showed no
   differences (P > 0.05); VFA concentration was different (P < 0.05), the
   diet with 45\% of Moringa oleifera had the best VFA concentration;
   however, the total biogas production and CH4 and CO2 concentrations were
   lower (P < 0.05) in the diet with 45\% Leucaena leucocephala. In
   conclusion, the addition of 45\% of Leucaena leucochephala to the diet
   improved chemical composition without affecting the VFA concentration
   and producing less CH4 and CO2 (18 and 24\%, respectively).},
DOI = {10.28940/terra.v38i2.618},
ISSN = {0187-5779},
EISSN = {2395-8030},
ResearcherID-Numbers = {Galicia Jimenez, Monica Marcela/F-1169-2018},
ORCID-Numbers = {Avila Serrano, Narciso Ysac/0000-0003-4043-501X
   Camacho-Escobar, Marco Antonio/0000-0001-9693-1026
   Galicia Jimenez, Monica Marcela/0000-0001-9361-5765},
Unique-ID = {WOS:000943016900015},
}

@article{ WOS:A1984SH81200024,
Author = {HODGES, CS and TENORIO, JA},
Title = {ROOT DISEASE OF DELONIX-REGIA AND ASSOCIATED TREE SPECIES IN THE MARIANA
   ISLANDS CAUSED BY PHELLINUS-NOXIUS},
Journal = {PLANT DISEASE},
Year = {1984},
Volume = {68},
Number = {4},
Pages = {334-336},
Abstract = {A root and butt rot of flame tree, D. regia, in the Mariana Islands was
   caused by the fungus Phellinus noxius. Several associated tree species,
   including Albizia lebbek, Bauhinia sp., Casuarina sp., Citrus sp.,
   Erythrina sp., Leucaena leucocephala and Thespesia populnea, were also
   infected by the fungus. The disease is characterized by slowly enlarging
   disease centers and a thick, dark brown mycelial sheath around the bases
   of infected trees. A serious root disease of breadfruit, Artocarpus
   altilis, previously reported from other island groups in the Western
   Pacific as caused by a Corticium sp., was caused by P. noxius.},
DOI = {10.1094/PD-69-334},
ISSN = {0191-2917},
EISSN = {1943-7692},
ResearcherID-Numbers = {Tenorio, Jair/AGD-3406-2022},
Unique-ID = {WOS:A1984SH81200024},
}

@article{ WOS:A1985ADE7200014,
Author = {NECHOLS, JR and SEIBERT, TF},
Title = {BIOLOGICAL-CONTROL OF THE SPHERICAL MEALYBUG, NIPAECOCCUS-VASTATOR
   (HOMOPTERA, PSEUDOCOCCIDAE) - ASSESSMENT BY ANT EXCLUSION},
Journal = {ENVIRONMENTAL ENTOMOLOGY},
Year = {1985},
Volume = {14},
Number = {1},
Pages = {45-47},
Abstract = {Survivorship of N. vastator (Maskell) was significantly higher on
   branches of Leucaena leucocephala (Lam.) DeWit. (Leguminosae) tended by
   the ant, Technomyrmex albipes F. Smith, than on branches from which ants
   were excluded. The presence of ants decreased the percentage of
   mealybugs parasitized by the encyrtid, Anagyrus indicus Shafee, Alam and
   Agarwal (Hymenoptera), and the mortality attributable to host killing by
   A. indicus and predation by other arthropods. Because ant interference
   occurs at only a few isolated localities in Guam, these experiments
   support the hypothesis that natural enemies play an important role in
   maintaining mealybug populations at low levels.},
DOI = {10.1093/ee/14.1.45},
ISSN = {0046-225X},
EISSN = {1938-2936},
Unique-ID = {WOS:A1985ADE7200014},
}

@article{ WOS:A1985ACR8500006,
Author = {SIEGEL, SM and SIEGEL, BZ},
Title = {DIFFERENTIAL ELIMINATION OF MERCURY DURING MATURATION OF LEGUMINOUS
   SEEDS},
Journal = {PHYTOCHEMISTRY},
Year = {1985},
Volume = {24},
Number = {2},
Pages = {235-236},
Abstract = {The pods of Acacia koa, Canavalia cathartica, Leucaena leucocephala,
   Samanea saman and 6 other leguminous species {[}Cassia glauca, C.
   bicapsularis, Delonix regia, Mezoneuron kauaiense, Crotalaria mucronata,
   Mucuna gigantea] lose as much as 75-85\% of their tissue water during
   maturation, but show no loss in tissue Hg content. Unlike fruit, seeds
   not only lost most of their water content, but also at least 75\% of
   their Hg during the maturation process. Selective elimination is more
   likely to take place by bio-volatilization rather than by translocation
   and the phenomenon allows for detoxification of a cytogenetically
   hazardous chemical substance.},
DOI = {10.1016/S0031-9422(00)83526-8},
ISSN = {0031-9422},
EISSN = {1873-3700},
