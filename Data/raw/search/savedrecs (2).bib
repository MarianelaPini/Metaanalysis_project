Number = {3},
Pages = {185-188},
Month = {MAY},
Abstract = {Leucaena leucocephala and Albizzia falcataria (sengon) together with
   corn stalks, an agricultural residue, were pulped by alkaline processes.
   Corn stalk was the easiest to pulp as it has a more open structure than
   Albizzia which is highly porous; the hardest to delignify was Leucaena
   since the lignin content was somewhat higher and the wood structure
   rather dense.  High hot water solubilities of corn stalks interfere with
   delignification at higher temperatures, due to reactions which lower pH
   and cause the redeposition of lignin increasing Kappa numbers.},
ISSN = {1038-6807},
Unique-ID = {WOS:A1992HX47200008},
}

@article{ WOS:A1988Q895900016,
Author = {UECKERT, DN and PETERSEN, JL and WAGNER, MW and LIVINGSTON, CW and
   ALLISON, MJ},
Title = {LITTLELEAF LEADTREE - A POTENTIAL FORAGE CROP FOR SHEEP AND GOATS},
Journal = {TEXAS AGRICULTURAL EXPERIMENT STATION PROGRESS REPORT},
Year = {1988},
Number = {4565},
Pages = {34-36},
Month = {SEP},
Abstract = {Rambouillet ewe lambs and Angora kids were used in grazing trials to
   evaluate the potential value of littleleaf leadtree (Leucaena retusa), a
   native, deciduous, woody legumen, as supplemental grazing for sheep and
   goats during summer and autumn. Mimosine, a toxic amino acid, was
   present at low concentrations in the foliage, but it occurred at lower
   concentrations than that reported for the tropical forage legumen
   Leucaena leucocephala. Lambs and kid goats gained weight in most grazing
   trials and there was no evidence of mimosine toxicity.},
Unique-ID = {WOS:A1988Q895900016},
}

@article{ WOS:A1989U936800001,
Author = {BAGYARAJ, DJ and REDDY, MSB and NALINI, PA},
