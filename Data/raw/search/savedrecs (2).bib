@article{ WOS:A1990DM90800005,
Author = {ZARRAGOITIA, L and ELIAS, A and RUIZ, TE and PLAZA, J and RODRIGUEZ, J},
Title = {THE UTILIZATION OF SACCHARINA AND LEUCAENA (LEUCAENA-LEUCOCEPHALA) AS
   SUPPLEMENT FOR GROWING BOVINE FEMALES UNDER NONIRRIGATED GRASSLAND
   CONDITIONS},
Journal = {CUBAN JOURNAL OF AGRICULTURAL SCIENCE},
Year = {1990},
Volume = {24},
Number = {1},
Pages = {45-52},
Month = {MAR},
Abstract = {The performance of growing bovine females from 12 months of age was
   evaluated in three treatments under non-irrigated grassland conditions
   from November 1988 until August 1989 (268 days). 40 Holstein females of
   200 kg live weight were used in a random block design distributed in
   three treatments: A) coast bermuda grass 68 (Cynodon dactylon) + 2 kg of
   concentrates with 58\% of Saccharina; B) coast bermuda grass 68
   associated with Leucaena and C) coast bermuda grass 68 associated with
   Leucaena + final molasses. From February to March grazing was limited to
   4 hr and ground freash sugar cane was supplied at a rate of 10
   kg/animal/day. The stocking rate was 4 animals/ha. A higher final weight
   (P < 0.05) was found in the treatment supplemented with Saccharina (389
   kg) in respect to the treatments supplemented with Leucaena (367 kg) and
   Leucaena + final molasses (369 kg). Live weight gain until incorporation
   to artificial insemination was 735 g/day for Saccharina and higher (P <
   0,01) for the treatments with Leucaena (611 and 622 g/day) without final
   molasses and with final molasses, respectively. With these treatments
   accumulated gains higher than 600 g/day were attained. No response to
   supplementation with final molasses diets with coast bermuda grass 68
   associated with Leucaena was found. The substitution of part of the
   cereals by Saccharina in diets for growing females and in coast bermuda
   68 grasslands is a useful alternative for animal production which merits
   further studies.},
ISSN = {0864-0408},
EISSN = {2079-3480},
Unique-ID = {WOS:A1990DM90800005},
}

@article{ WOS:A1984TY83200007,
Author = {ARIGAYO, S and FUJISAWA, S and SAKURAI, A and KAMIYA, Y and ADISEWOJO,
   SS and TAKAHASHI, N},
Title = {FLUCTUATION OF ENDOGENOUS LEVELS OF FREE AND CONJUGATED GIBBERELLINS
   THROUGHOUT SEED MATURATION IN LEUCAENA-LEUCOCEPHALA (LMK) DE WIT},
Journal = {PLANT AND CELL PHYSIOLOGY},
Year = {1984},
Volume = {25},
Number = {8},
Pages = {1395-1402},
Abstract = {Combined gas chromatography-selected ion monitoring (GC-SIM) analysis of
   a purified extract from seeds of L. leucocephala showed the presence of
   GA1, GA8, GA17, GA19, GA20, GA23, GA29 and GA53. GA1, GA8 and GA29 were
   also found both as glucosyl ester-like and glucosyl ether-like
   conjugates, and GA20 as a glucosyl ester-like conjugate; these
   conjugates were analyzed as free GA after enzyme hydrolysis. GA23 was
   the main free GA in immature seeds (268 ng/seed), though its level
   drastically decreased during their maturation. GA1 was the main free
   C19-GA and GA1 glucosyl ester-like and glucosyl ester-like conjugates
   were found at the highest levels in the seeds prior to maturation.
   Fluctuation of endogenous levels of GA is discussed in terms of seed
   development.},
