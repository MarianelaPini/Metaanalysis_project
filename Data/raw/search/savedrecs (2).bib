@article{ WOS:A1981ND33600009,
Author = {CHAGAS, JM},
Title = {LEUCAENA LEUCOCEPHALA AS A GREEN MANURE FOR BEAN GROWING IN CERRADO SOIL},
Journal = {PESQUISA AGROPECUARIA BRASILEIRA},
Year = {1981},
Volume = {16},
Number = {6},
Pages = {809-814},
Abstract = {Two trials were undertaken to evaluate (L. leucocephala (Lam.) de Wit.)
   as a green manure in a Dark-Red Latosol under cerrado vegetation in the
   municipality of Goiania, Goias, Brazil. In the 1st trial, the green
   manure was seeded with spacing of 2, 3, 4 and 5 between rows, with 30
   seeds/m. An analysis of growth showed an increase in the production of
   dry matter/ha and in N, P, K, Ca and Mg per sample as spacing decreased.
   In the 2nd trial, P. vulgaris L. were planted between the rows of
   leucena, after pruning of lateral shoots, leaving only the main stalk
   cut at a height of 1.5 m. Treatments consisted of the incorporation or
   non-incorporation of the leucena, combined with N (30 k/ha of N) or P
   (120 k/ha of P2O5) or N + P. The incorporation of leucena in the soil
   alone resulted in a production of beans equivalent to that of the
   treatments which received chemical fertilization.},
ISSN = {0100-204X},
EISSN = {1678-3921},
Unique-ID = {WOS:A1981ND33600009},
}

@article{ WOS:A1977DU47400006,
Author = {BLUNT, CG and JONES, RJ},
Title = {STEER LIVEWEIGHT GAINS IN RELATION TO PROPORTION OF TIME ON
   LEUCAENA-LEUCOCEPHALA PASTURES},
Journal = {TROPICAL GRASSLANDS},
Year = {1977},
Volume = {11},
Number = {2},
Pages = {159-164},
Abstract = {Kimberley shorthorn weaner steers stocked at 5.6 ha-1 initially made
   good gains (0.9 kg day-1) on irrigated leucaena{[}L. leucocephala cv.
   Peru (leucaena)]-pangola {[}Digitaria decumbens] pastures but
   subsequently gained poorly. Over 308 days they gained a mean of only
   0.29 kg day-1. A pasture rotation of 1 wk grazing and 3 wk rest,
   designed to produce feed lower in mimosine, did not alleviate the
   situation compared with a rotation of 1 wk graze and 1 wk rest. When
   steers were moved from leucaena pasture to a pangola pasture they gained
   poorly compared with those grazing only pangola grass. On returning to
   leucaena pastures they grew much faster than those grazing leucaena
   pasture only. Overall gains were very similar. Toxicity ratings were
   related to the time spent on leucaena. In a subsequent trial steers
   grazed leucaena or pangola for 224 days and gained, respectively, 0.29
   and 0.46 kg day-1. When common grazed on leucaena they gained 0.24 and
   0.62 kg day-1, respectively. Animals grazing leucaena develop a chronic
   toxicity which depresses liveweight gain. The toxicity may be overcome
   by removing animals to pasture free of leucaena. Genetic potential of
   the Kimberley shorthorn cattle in this hot tropical environment could
   not be implicated since these animals gained 0.9 kg head-1 day-1 on a
   feedlot ration.},
ISSN = {0049-4763},
Unique-ID = {WOS:A1977DU47400006},
}

