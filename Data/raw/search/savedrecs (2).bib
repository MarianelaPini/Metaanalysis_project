@article{ WOS:000955320000001,
Author = {Hueza, Isis Machado and Dipe, Vanius Vinicius and Gotardo, Andre Tadeu
   and Gardner, Dale R. and de Almeida, Elaine Renata Motta and Gorniak,
   Silvana Lima},
Title = {Potential immunomodulatory response associated with L-mimosine in male
   Wistar rats},
Journal = {TOXICON},
Year = {2023},
Volume = {226},
Month = {APR},
Abstract = {Leucaena leucocephala is a plant that is used as animal and human food
   worldwide. This plant contains the toxic compound namely L-mimosine. The
   main mechanism of action of this compound involves its ability to
   chelate metal ions, which may interfere with the proliferative activity
   of cells and being studied for the treatment of cancer. However, little
   is known about the effect of L-mimosine on immune responses. Thus, the
   aim of this study was to evaluate the effects of L-mimosine on immune
   responses in Wistar rats. Different doses of L-mimosine (25, 40 and 60
   mg/kg body weight/day) were administered orally by gavage to adult rats
   for 28 days. No clinical signs of toxicity were observed in animals, but
   a decrease in the T-dependent response to sheep red blood cells (SRBC)
   in animals treated with 60 mg/kg L-mimosine and an increase in the
   intensity of S. aureus phagocytosis by macrophages in animals treated
   with 40 or 60 mg/kg L-mimosine were observed. Therefore, these findings
   suggest that L-mimosine did not compromise macrophage activity and
   inhibited T-dependent clonal expansion during the immune response.},
DOI = {10.1016/j.toxicon.2023.107084},
EarlyAccessDate = {MAR 2023},
Article-Number = {107084},
ISSN = {0041-0101},
EISSN = {1879-3150},
