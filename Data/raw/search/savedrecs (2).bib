@article{ WOS:000175450400005,
Author = {Verma, PRP and Razdan, B},
Title = {Evaluation of leucaena leucocephala seed gum in tabletting II. Binding
   properties in granules and tablets},
Journal = {STP PHARMA SCIENCES},
Year = {2002},
Volume = {12},
Number = {2},
Pages = {113-119},
Month = {MAR-APR},
Abstract = {Leueaena gum derived from seeds of Leueaena leucocephala was evaluated
   as a binding agent in lactose-based tablets containing sodium
   salicylate. The binding properties of gum were compared with acacia and
   methyl cellulose at equivalent concentrations. The following granular
   properties were evaluated: densities, Hausner ratio, Carr's index, angle
   of repose, flow rate and percentage of fines. The tablet properties
   investigated included: uniformity of weight, hardness, friability,
   binding capacity, disintegration time and in vitro dissolution (k,
   t(50\%), A(30)). Like other binding agents, an increase in gum
   concentration decreases the percentage fines and friability and
   increases the hardness and disintegration time. Furthermore, the greater
   the binding concentration, the lower the k, A(30) and the higher the
   t(50\%). The results indicate that Leucaena seed gum is a potential
   pharmaceutical binding agent.},
ISSN = {1157-1489},
ResearcherID-Numbers = {Verma, Rahul/C-6027-2012},
Unique-ID = {WOS:000175450400005},
}

@article{ WOS:000080263700014,
Author = {Mesa, D and Romero, A and Cruz, AM},
Title = {Preliminary study on indicators for the in vitro micropropagation of
   Leucaena leucocephala cv. Peru},
Journal = {CUBAN JOURNAL OF AGRICULTURAL SCIENCE},
Year = {1998},
Volume = {32},
Number = {4},
Pages = {393-397},
Month = {DEC},
Abstract = {Studies to determine the most efficient seed disinfection and
   scarification methods, growth dynamics and culture media for the in
   vitro micropropagation of L. leucocephala cv. Peru, were carried out.
   Six disinfection periods with 1\%sodium hypochlorite (5, 10, 15, 20, 25,
   30 min) and mercury bichloride as the control. The scarification was
   carried out with hot water for 3 min, seed pre-germination 24 hr in
   sterile water at room temperature and a control treatment. A simple
   classification design with 3 replications was used. Daily measurements
   of height were taken from the in vitro sowing until 14 days and from
   these plants 9 explants were dissected for their analysis. Also, the
   response to two culture media in plant formation was tested. In the seed
   disinfection the best response was obtained with the mercury bichloride
   where contamination reached only 23.7\%. However, with sodium
   hypochlorite these ranged from 65.95 to 77.49\% (P < 0.001). The most
   effective scarification was seed pre-germination in water for 24 hr with
   germination values of 77.08\%, while in hot water it was of 59.2\% (P <
   0.001). The linear equation,for growth (y = 0.993 + 0.503x) was of good
   fit (r(2) = 0.99). Of the explants studied only the cotyledonal nodes
   formed plants and showed a superior response with the concentration of 5
   mg/litre of BAP. Further studies on disinfection, scarification and
   hormone concentration for the in vitro micropropagation of L.
   leucocephala are suggested.},
ISSN = {0864-0408},
ORCID-Numbers = {Mesa Travieso, Dianelis/0000-0001-6076-0579},
Unique-ID = {WOS:000080263700014},
}

@article{ WOS:A1984TJ64200011,
Author = {RAO, TC and LAKSHMINARAYANA, G and PRASAD, NBL and RAO, SJM and
