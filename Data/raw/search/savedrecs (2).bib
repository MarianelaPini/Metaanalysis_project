@article{ WOS:000079328900003,
Author = {Barnes, P},
Title = {Fodder production of some shrubs and trees under two harvest intervals
   in subhumid southern Ghana},
Journal = {AGROFORESTRY SYSTEMS},
Year = {1998},
Volume = {42},
Number = {2},
Pages = {139-147},
Abstract = {The study was undertaken to determine the relative performance of 13
   fodder trees and shrubs in herbage production and quality under two
   forage regrowth periods six and 12 weeks, Ln primary growth assessment
   it was found that the species which achieved the most height growth were
   Cajanus cajan, Sesbania sesban and Gliricidia sepium. In terms of
   herbage dry matter production the species which performed best were
   Cajanus cajan, Fleminigia macrophylla Calliandra calothrysus,
   Codariocalyx gyroides and Gliricidia sepium. In crude protein content
   the superior species were Albizia lebbeck. Cajanus cajan, Leucaena
   leucocephala and Sesbania sesban. Regrowth harvests in the second year
   showed Gliricidia sepium, Calliandra calothrysus and Flemingia
   macrophylla achieve the greatest herbage yields with yields in excess of
   1 t/ha obtaining at the first 12 week regrowth harvest. Albizia lebbeck,
   Cajanus cajan, Leucaena leucocephala and Samanean saman produced
   consistently very high crude protein levels (more than 20\%) in the
   regrowth harvests. Calcium contents of herbage of all test species in
   the regrowth harvests were in excess of the critical minimum requirement
   of 0.44\% for ruminant feeding.},
DOI = {10.1023/A:1006194311124},
ISSN = {0167-4366},
EISSN = {1572-9680},
Unique-ID = {WOS:000079328900003},
}

@article{ WOS:000865425400002,
Author = {Bandara, W. A. R. T. W. and Ranasinghe, Oshadhi and Perera, Priyan and
   Vlosky, Richard and Kizha, Anil Raj},
Title = {Potential to use invasive plants in biomass energy production: A case
   study Prosopis juliflora in coastal wetlands of Sri Lanka},
Journal = {TREES FORESTS AND PEOPLE},
Year = {2022},
Volume = {10},
Month = {DEC},
Abstract = {Prosopis juliflora is an invasive plant species rapidly expanding in the
   Asian and African continents. The invasion of P. juliflora in Bundala
   Ramsar Wetland (BRW) in Sri Lanka has created several biodiversity
   conservation issues. This study was conducted to assess the possibility
   of utilizing invasive P. juliflora as an alternative energy source. P.
   juliflora performed better than L. leucocephala for most evaluated
   fuelwood properties. Ash content was comparatively higher in P.
   juliflora than L. leucocephala. However, biomass to ash ratio of P.
   juliflora was significantly lower (<0.05) and the Fuel Value Index (FVI)
   of P. juliflora (3,276) was slightly lower than that of L. leucocephala
   (3,336), a non-significant difference. P. juliflora and L. leucocephala
   reached Fiber Saturation Point within 24 and 27 days of drying,
   respectively. Results show that 1 kg of P. juliflora would produce an
   estimated energy equivalent to 0.5 L of diesel and furnace oil and 5 kWh
   of electricity. As such, we recommend further study on harvesting and
   commercialization of P. juliflora as a potential wood energy source.},
DOI = {10.1016/j.tfp.2022.100330},
EarlyAccessDate = {SEP 2022},
Article-Number = {100330},
EISSN = {2666-7193},
ResearcherID-Numbers = {Perera, Priyan/K-2352-2012
   },
ORCID-Numbers = {Perera, Priyan/0000-0002-9452-4790},
Unique-ID = {WOS:000865425400002},
}

@article{ WOS:000170572000011,
Author = {Arya, RL and Niranjan, KP and Singh, A and Singh, JB},
Title = {Effect of manurial schedules on growth and yield of different cropping
   systems under rainfed alley system},
Journal = {INDIAN JOURNAL OF AGRONOMY},
Year = {2000},
Volume = {45},
Number = {4},
Pages = {687-692},
Month = {DEC},
Abstract = {A field experiment was conducted during the rainy season of 1993-94 to
   1995-96 at Jhansi, to study the different food-fodder cropping systems
   and manurial schedules under rainfed conditions. Significantly higher
   grain and stover yields of sorghum {[}Sorghum bicolor (L.) Moench] were
   obtained in sorghum + pigeonpea {[}Cajanus cajan (L.) Millsp.]
   intercropping system as compared with other intercropping systems.
   Sorghum + cowpea {[}Vigna unguiculata (L.) Walp.] intercropping gave
   significantly more green fodder and dry-matter yields and sorghum
   grain-equivalent yield than the other intercropping systems. Among the
   different manurial schedules, combined application of organic and
   inorganic sources of plant nutrients resulted in significantly higher
   grain and stover yields of sorghum, green fodder and dry-matter yields
   of fodder crops and sorghum grain-equivalent yield as compared to 100\%
   through inorganic fertilizers application.},
ISSN = {0537-197X},
Unique-ID = {WOS:000170572000011},
}

@inproceedings{ WOS:000180110900033,
Author = {Al-Badawy, AA and Hussien, ESH},
Editor = {Mackay, WA},
Title = {Influence of growth regulator treatments on growth of some woody plants},
Booktitle = {PLANT GROWTH REGULATION SOCIETY OF AMERICA, PROCEEDINGS},
Year = {2002},
Pages = {143-149},
Note = {29th Annual Meeting of the Plant-Growth-Regulation-Society-of-America,
   HALIFAX, CANADA, JUL 28-AUG 01, 2002},
Organization = {Plant Growth Regulat Soc Amer; Acadian Seaplants Ltd; AMVAC Chem Corp;
   Aventis CropSci; Dormex Co USA LLC; Dynamac Corp; Griffin LLC; PBI
   Gordon Corp; SePRO Corp; Stoller Enterprises Inc; Syngenta Profess Prod;
   Uniroyal Chem Co},
Abstract = {Two separate experiments were conducted to study the response of lead
   tree (Leucaena leucocephala (Lam.) de Wit.) and sweet Acacia (Acacia
   farnesiana L.) to the application of some plant growth regulator
   treatments. Seedlings of Leucaena leucocephala and Acacia farnesiana
   grown in 25-cm pots filled with sand and peat moss (1:1 v/v) were
   sprayed three times at two-week intervals with Gibberellic acid
   {[}GA(3)]at 0, 50, 100 or 150 ppm, or Butanediodic acid
   mono(2,2-dimethylhyrazide) {[}B-9] at 0,250, 500, or 1000 ppm or treated
   with beta-{[}(4-chlorophe nyl)methyl]-alpha-(1,1-dimethylethyl)-
   1H-1,2,4-triazole-1-ethanol {[}Bonzi] as soil drenches at 0, 1, 5, or 10
   ppm.
   The obtained results showed that GA(3) treatments increased plant
   height, stem elongation and plant dry weight of the treated seedlings
   compared to the untreated ones. However, B-9 and Bonzi treatments tended
   to shorten the plant height and stem elongation and, in some cases,
   increased plant dry weight and root growth, The effects of these growth
   regulators on the N, P, and K concentration were varied with the plant
   species, the applied growth regulator and its concentration.},
Unique-ID = {WOS:000180110900033},
}

@article{ WOS:A1996TX40900041,
