@article{ WOS:A1989AQ06700005,
Author = {PRASAD, J and PALIWAL, OP},
Title = {PATHOLOGICAL-CHANGES IN EXPERIMENTALLY INDUCED LEUCAENA TOXICITY IN
   LAMBS},
Journal = {INDIAN VETERINARY JOURNAL},
Year = {1989},
Volume = {66},
Number = {8},
Pages = {711-714},
Month = {AUG},
Abstract = {Leucaena toxicity was experimentally produced in lambs by ad lib:
   feeding of Leucaena leucocephala (4.34.+-.0.29\% mimosine) daily. Gross
   changes comprised of ulcers on tongue, enlarged thyroids and
   haemorrhages in the kidneys and liver. Histologically the thyroids
   showed hyperplasia and proliferation of acini. Ulceration was prominent
   on the tongue. Congestion, haemorrhages and necrosis of hepatic cells
   were observed around the central vein in the liver. Kidneys revealed
   acute congestion of glomeruli, tubular degeneration and necrosis. The
   histopathological lesions of liver, kidneys, thyroids and tongue were
   suggestive of toxicity of leucaena.},
ISSN = {0019-6479},
EISSN = {0974-9365},
Unique-ID = {WOS:A1989AQ06700005},
}

@article{ WOS:000235715500007,
Author = {Ndemanisho, EE and Kimoro, BN and Mtengeti, EJ and Muhikambele, VRM},
Title = {The potential of Albizia lebbeck as a supplementary feed for
   goats in tanzania},
Journal = {AGROFORESTRY SYSTEMS},
Year = {2006},
Volume = {67},
Number = {1},
Pages = {85-91},
Month = {APR},
Abstract = {Forty growing goats (20 males and 20 females) were used in a 90-day
   growth and intake study to evaluate the potential of Albizia lebbeck
   (ABC) based concentrate as compared with Gliricidia sepium (GBC),
   Leucaena leucocephala (LBC), and Moringa oleifera (MBC) based
   concentrates with cotton seed cake (CSC) as a control. The animals were
   fed a basal diet of maize stover and supplemented daily with 30 g/kg
   W-0.75 of ABC, GBC, LBC, MBC, and CSC. They were compounded so as to
   formulate iso-protein diets. Treatment effects were significant (p <
   0.05) for growth rates in that with the exception of GBC, goats on CSC
   exhibited significantly higher gains compared to counterparts in other
   treatments. The total DM intake was between 50.6 and 52.6 g/kg
   W-0.75/day and there were no significant (p > 0.05) differences among
   treatments. There were significant differences (p < 0.05) among
   treatments for maize stover intake. CSC based treatment showed the
   highest intake with insignificant (p > 0.05) differences amongst the
   leaf meal based concentrates. It can be concluded that Albizia lebbeck
   had similar potential to the other leaf meal based supplements studied
   but had lower potential compared to a cotton seed cake based supplement,
   as protein sources for growing goats fed a poor quality basal diet.},
DOI = {10.1007/s10457-005-2648-1},
ISSN = {0167-4366},
EISSN = {1572-9680},
Unique-ID = {WOS:000235715500007},
}

@inproceedings{ WOS:000082289100024,
Author = {Bolam, MJ and Triglone, T},
Editor = {Shelton, HM and Gutteridge, RC and Mullen, BF and Bray, RA},
Title = {Evaluation of Leucaena germplasm in northern Western Australia},
Booktitle = {LEUCAENA - ADAPTATION, QUALITY AND FARMING SYSTEMS},
Series = {ACIAR PROCEEDINGS SERIES},
Year = {1998},
Number = {86},
Pages = {163-165},
Note = {Workshop on Leucaena - Adaptation, Quality and Farming Systems, HANOI,
   VIETNAM, FEB 09-14, 1998},
Organization = {Australian Ctr Int Agr Res; Crawford Fund Int Agr Res; Vietnam Natl
   Univ; Univ Queensland, Australia},
Abstract = {Twenty-two taxa belonging to the genus Leucaena were assessed for their
   agronomic potential under irrigation in northern Western Australia.
   Survival of seedlings, height, dry matter production and insect damage
   were recorded over a 27-month period. There was considerable variability
   in the survival of seedlings with some species being poorly adapted.
   Edible dry matter production was greatest for Leucaena leucocephala cv.
   Cunningham and cv. Tarramba, three interspecific hybrids, and L.
   salvadorensis 36/88. Further evaluation for potential production under
   grazing is needed. No psyllids were observed, but thrips had a variable
   effect on plant growth immediately after harvest.},
ISBN = {1-86320-232-3},
Unique-ID = {WOS:000082289100024},
}

@article{ WOS:A1995QH20000014,
Author = {PINTO, JCS and REMACLEVOLON, G and SAMPAIO, CAM and DAMAS, J},
Title = {COLLAGENASE-INDUCED EDEMA IN THE RAT PAW AND THE KININ SYSTEM},
Journal = {EUROPEAN JOURNAL OF PHARMACOLOGY},
Year = {1995},
Volume = {274},
Number = {1-3},
Pages = {101-107},
Month = {FEB 14},
Abstract = {Collagenase (100 mu g) induced a large plasma extravasation, during the
   first 15 min after its injection in rat paw, associated with the rapid
   development of oedema which subsided after 6 h. The extent of the oedema
   was similar in normal and kininogen-deficient rats. The swelling induced
   in normal rats was reduced by HOE 140
   (D-Arg{[}Hyp(3),Thi(5),D-Tic(7),Oic(8)]bradykinin), a bradykinin B-2
   receptor antagonist, and by three serine protease inhibitors, soybean
   trypsin inhibitor (SBTI), Leucaena leucocephala trypsin inhibitor 1
   (LLTI-1) and Leucaena leucocephala trypsin inhibitor 2 (LLTI-2). These
   agents had no effect on the oedema induced in kininogen-deficient rats.
   The swelling was also reduced by methysergide, indomethacin, ketoprofen
   and methylprednisolone. It was increased by heparin, but it was not
   modified by mepyramine, WEB 2086 (3-{[}4-(2-chlorophenyl)-9
   methyl-6H-thieno{[}3,2-f]{[}1,2,4]-triazolo-{[}4,3-a]{[}1,4]-diazepine-2
   -yl]-1-(4-morpholinyl)-1-propanone) and N-G-nitro-L-arginine. In vitro,
   collagenase did not release kinins from rat plasma or from purified
   T-kininogen. LLTI-1 and LLTI-2 did not inhibit collagenase activity for
   one of its specific substrates. Kinins are thus involved in the
   development of collagenase oedema in normal rats. Their generation would
   be indirect following changes in matrix proteins in extravascular
   spaces. Nevertheless, kinins are not the decisive mediators of the
   swelling. Serotonin, possibly released from platelets, and prostanoids
   participate in the inflammatory process.},
DOI = {10.1016/0014-2999(94)00723-K},
ISSN = {0014-2999},
ResearcherID-Numbers = {Pinto, José/AAE-9870-2019},
Unique-ID = {WOS:A1995QH20000014},
}

@article{ WOS:000181680400015,
Author = {Rodríguez, I and Crespo, G and Rodríguez, C and Castillo, E and Fraga, S},
Title = {Performance of soil macrofauna in pure natural grasslands or
   intercropped with leucaena for bull fattening},
Journal = {CUBAN JOURNAL OF AGRICULTURAL SCIENCE},
Year = {2002},
Volume = {36},
Number = {2},
Pages = {173-177},
Month = {JUN},
Abstract = {For three consecutive years, the performance of soil macrofauna was
   studied in two types of grasslands with fattening cattle: a) native
   grassland, b) native grassland with Leucaena leucocephala in 100 \% of
   the area. The individuals number and biomass of the soil macrofauna were
   determined, as well as its temperature and humidity percentage. A linear
   model of fixed effects that controlled the type of grassland, the year
   and the grassland x year interaction was used. The results showed a
   higher biomass value (53.66 versus 31.36 g/m(2)) in the leucaena system,
   compared to the native grassland. There was a significant increase of
   the soil humidity percentage in the b) system. Soil temperature and the
   number of macrofauna individuals showed interaction (P < 0.01) between
   the grassland types and the studied years. Three phyla (Arthropods,
   Annelida and Mollusca), five classes and seven orders were identified in
   the diversity of the groups. The presence of three genera of the phylum
   Annelida (Polypheretima, Onychochaeta and Diplotrema) was found. It is
   concluded that the high biomass values of the edaphic fauna found in the
   system with Leucaena leucocephala is due, among other factors, to the
   presence of a greater plant diversity, a larger amount of better quality
   leaf litter and a more favorable microenvironment for macrofauna
   activity due to a higher soil humidity. The introduction of leucaena in
   livestock production systems is recommended since it supplies shade and
   feed to the animals and stimulates and diversifies the soil biological
   activity.},
ISSN = {0864-0408},
Unique-ID = {WOS:000181680400015},
}

@inproceedings{ WOS:A1997BJ28P00003,
Author = {Daniel, JN and Hedge, NG and Relwani, LL},
Editor = {Zabala, NQ},
Title = {Performance of Albizia lebbeck in semiarid India},
Booktitle = {INTERNATIONAL WORKSHOP ON ALBIZIA AND PARASERIANTHES SPECIES},
Series = {FOREST, FARM, AND COMMUNITY TREE RESEARCH REPORTS},
Year = {1997},
Pages = {22-26},
Note = {International Workshop on Albizia and Paraserianthes Species, BISLIG,
   PHILIPPINES, NOV 13-19, 1994},
Organization = {Winrock Int Inst Agr Dev; UN Dev Program, FOA Reg Forest Tree
   Improvement Project; PICOP Resources Inc},
Abstract = {Albizia lebbeck is commonly found in the semiarid regions of India, but
   its potential is not fully exploited in a systematic manner. In species
   comparison studies in three sites receiving medium to low rainfall, rf,
   lebbeck performed consistently well. It has also been found to be a
   potential species for agroforestry as it intercepts less light than
   Leucaena leucocephala of comparable age. However, because of the
   spreading nature of its branches, appropriate pruning practices may be
   required when the trees mature. Quality planting material and pest and
   disease management are other requirements: to increase the use Of this
   species.},
Unique-ID = {WOS:A1997BJ28P00003},
}

@article{ WOS:000511130900001,
Author = {Mata-Balderas, Jose Manuel and Eduardo Hernandez-Cardenas, Silvino and
   Alanis-Rodriguez, Eduardo and Mora-Olivo, Arturo},
Title = {Richness, composition and abundance of species in a riparian plant
   community in the Santa Catarina river, Monterrey, Nuevo Leon},
Journal = {CIENCIAUAT},
Year = {2020},
Volume = {14},
Number = {2},
Pages = {6-20},
Month = {JAN-JUN},
Abstract = {Riparian vegetation constitutes a key element for the maintenance of
   fluvial ecosystems, especially for those that are surrounded by ur{''}
   Jan zones. The aim of this study was to evaluate the composition and
   variation of riparian vegetation in a stretch of the Santa Catarina
   river, over two cities of the Monterrey Metropolitan Area. For the
   analysis of the vegetation, sampling transects were established in a
   section of 15.65 km. The high stratum (shrub and tree plants) was
   analyzed using a Bray-Curtis ordering model and a dominance-diversity
   plot. The Jaccard similarity coefficient was used for the low stratum
   (herbaceous plants). In the high stratum, 16 families belonging to 25
   genera and 28 species were recorded. The species with the highest number
   of individuals were Salix nigra, Leucaena leucocephala, Ricinus communis
   and Parkinsonia aculeata. The composition of the vegetation has a
   similarity level of intermediate to high. because the groups vary
   between 60 \% to 71.53 \%. In thelow stratum. 25 families. 53 genera and
   60 species were recorded with a similarity level of low to intermediate,
   because the groups vary between 23 to 47 \%. The species that were
   mostly registered in the low stratum were Cyperus involucratus in 7
   sites, and Eragrostis mexicana, Heliantus annuus, Melinis repens,
   Polygonum pensylvanicum and Typha domingensis in six sites each one. The
   composition of the vegetation is in a secondary successional state,
   because the species with greater presence are exotic or invasive, such
   as Leucaena leucocephala and Ricinus communis for the high stratum and
   Cyperus involucratus, Typha aomingensis and Melinis repens for the low
   stratum.},
DOI = {10.29059/cienciauat.v14i2.1248},
ISSN = {2007-7521},
EISSN = {2007-7858},
ResearcherID-Numbers = {Mora-Olivo, Arturo/GXN-3405-2022},
Unique-ID = {WOS:000511130900001},
}

@article{ WOS:000177494300009,
