@article{ WOS:A1993LT13900008,
Author = {HABTE, M and FOX, RL},
Title = {EFFECTIVENESS OF VAM FUNGI IN NONSTERILE SOILS BEFORE AND AFTER
   OPTIMIZATION OF P IN SOIL SOLUTION},
Journal = {PLANT AND SOIL},
Year = {1993},
Volume = {151},
Number = {2},
Pages = {219-226},
Month = {APR},
Abstract = {Five tropical soils were either not inoculated or inoculated with the
   vesicular-arbuscular mycorrhizal (VAM) fungus Glomus aggregatum. The
   degree to which VAM effectiveness was expressed in the soils was
   evaluated prior and after solution P status was adjusted for optimal VAM
   activity. VAM effectiveness determined by monitoring P concentrations of
   pinnules of Leucaena leucocephala leaves as a function of time and as
   dry matter yield determined at the time of harvest, indicated that in
   three of the soils VAM effectiveness was either very restricted or
   altogether unexpressed irrespective of vesicular-arbuscular mycorrhizal
   fungal (VAMF) inoculation if soil solution P was not optimized for VAM
   effectiveness. After P optimization, effectiveness was significantly
   increased by VAMF inoculation although in four of the soils, densities
   of indigenous VAMF propagules greatly exceeded that attained by the
   inoculum after it was mixed with soil. Mycorrhizal fungal inoculation
   effects varied from soil to soil, depending on the extent to which the
   effectiveness of indigenous and introduced endophytes was enhanced by P
   optimization and the similarity of inherent soil solution P
   concentrations to the range known to be optimum for VAM effectiveness.
   Of the indicator variables monitored, VAMF colonization was least
   sensitive to treatment effects followed by shoot P concentration
   measured at the time of harvest.},
DOI = {10.1007/BF00016287},
ISSN = {0032-079X},
EISSN = {1573-5036},
Unique-ID = {WOS:A1993LT13900008},
}

@article{ WOS:A1993MG31300009,
Author = {DOVEL, RL and VIETOR, DM and WEAVER, RW},
Title = {EFFECTS OF MEDIA-N CONTENT AND RHIZOBIAL STRAIN ON N-2 FIXATION AND
   PARTITIONING IN LEUCAENA SEEDLINGS},
Journal = {JOURNAL OF RANGE MANAGEMENT},
Year = {1993},
Volume = {46},
Number = {6},
Pages = {512-515},
Month = {NOV},
Abstract = {Research was conducted to examine the effect of Rhizobial race and N
   fertilization on N-2 fixation and N partitioning in seedlings of the
   genus Leucaena. Seedlings of Leucaena leucocephala (Lam) Dewit variety
   K-8 and L. retusa Gray variety Yellow Puff were grown in slender tubes
   filled with fritted clay and watered with nutrient solutions containing
   0, 2, or 8 mM nitrate N labeled with 0.1\% N-15. The seedlings were
   inoculated with Rhizobia loti race 94A3 or R. loti race 9408. After 12
   weeks the plants were harvested and the effects of N fertilization and
   Rhizobial race on nodulation N-2 fixation, and N partitioning were
   examined. Using the N-15-dilution method total N fixed by dinitrogen
   fixation was determined as well as distribution of fixed versus
   fertilizer N within the plant. Both Rhizobial races infected both
   Leucaena species resulting in an effective symbiosis. However, the 2
   Leucaena species responded quite differently to N fertilization. The
   addition of 2 mM N to the nutrient solution effectively eliminated
   nodulation in L. retusa yet increased both nodulation and N-2 fixation
   in L. leucocephala. The 8 mM N rate eliminated nodulation in both
   species. Due to the elimination of nodulation and N-2 fixation by N
   fertilization in L. retusa, the effect of N fertilization on
   partitioning of fixed N was only examined in L. leucocephala. Increasing
   the N content of the nutrient solution increased the proportion of total
   N in the shoot and reduced the proportion in nodules. A larger
   proportion of the fixed N was retained in the root and nodules than
   fertilizer N. Although the 2 mM N treatment increased the amount of N-2
   fixed in L. leucocephala compared to the 0 mM treatment, the proportion
   of fixed N-2 was reduced to less than one third of the total N in the
   plant.},
DOI = {10.2307/4002862},
ISSN = {0022-409X},
Unique-ID = {WOS:A1993MG31300009},
}

@article{ WOS:A1984TA63100012,
Author = {BRAY, RA},
Title = {EVALUATING HYBRIDS BETWEEN LEUCAENA-LEUCOCEPHALA AND
   LEUCAENA-PULVERULENTA AS POTENTIAL FORAGE PLANTS},
Journal = {AUSTRALIAN JOURNAL OF EXPERIMENTAL AGRICULTURE},
Year = {1984},
Volume = {24},
Number = {126},
Pages = {379-385},
Abstract = {The growth of 2 lines of L. leucocephala, 4 lines of L. pulverulenta and
   19 hybrids between the 2 spp. was studied in the glasshouse and in the
   field. Although the hybrids initially grew more slowly than the control
   L. leucocephala lines, most outgrew them in the field, yielding up to
   50\% more edible material (with .apprx. 80\% of the mimosine {[}toxic to
   livestock] level), and up to 100\% more non-edible material. These
   hybrids have considerable potential, especially as dual purpose 
   (forage/fuel) plants. A strategy for producing hybrid seed under field
   conditions is proposed.},
DOI = {10.1071/EA9840379},
ISSN = {0816-1089},
EISSN = {1836-5787},
Unique-ID = {WOS:A1984TA63100012},
}

@article{ WOS:A1984SU65700004,
Author = {TANGENDJAJA, B and LOWRY, JB and WILLS, RBH},
Title = {OPTIMIZATION OF CONDITIONS FOR THE DEGRADATION OF MIMOSINE IN
   LEUCAENA-LEUCOCEPHALA LEAF},
Journal = {JOURNAL OF THE SCIENCE OF FOOD AND AGRICULTURE},
Year = {1984},
Volume = {35},
Number = {6},
Pages = {613-616},
Abstract = {The rate of degradation of mimosine to 3-hydroxy-4(1H)-pyridone in
   harvested leucaena leaf was studied under various conditions. Optimum
   rates of degradation of an aqueous leucaena slurry were at pH 8.0 and
   45.degree. C with virtual total loss of mimosine in 10 min. Heating the
   intact leaf also reduced the mimosine content with the maximum rate of
   degradation at 70.degree. C. The results have implications for reducing
   the toxicity of leucaena used for stock feed.},
DOI = {10.1002/jsfa.2740350605},
ISSN = {0022-5142},
EISSN = {1097-0010},
ORCID-Numbers = {Wills, Ronald/0000-0002-0815-080X
   Tangendjaja, Budi/0000-0003-0110-4947},
Unique-ID = {WOS:A1984SU65700004},
}

@article{ WOS:A1982QF13700015,
Author = {SINGHAL, PC and GUPTA, RK and SINGH, JB and JOSHI, LD},
Title = {PRELIMINARY STUDIES ON HYPOGLYCEMIC AND HYPOCHOLESTEROLEMIC ACTIVITIES
   OF LEUCAENA-LEUCOCEPHALA (LAM) DEWIT},
Journal = {INDIAN JOURNAL OF MEDICAL RESEARCH},
Year = {1982},
Volume = {76},
Number = {S},
Pages = {119-123},
Abstract = {Powdered seed of L. leucocephala (Koo-Babul) a fast growing leguminous
   tree was fed to normal, diabetic and hypercholesterolemic rats. Blood
   sugar and serum cholesterol levels in the rats were lowered
   significantly. The hypoglycemic and hypocholesterolemic fractions of the
   whole seeds are insoluble in water, and therefore need extensive
   treatment with different organic solvents to extract the 2 fractions.
   The seed contains mimosine, a toxic non-protein amino acid, which needs
   to be subjected to a detailed toxicological study.},
ISSN = {0971-5916},
Unique-ID = {WOS:A1982QF13700015},
}

