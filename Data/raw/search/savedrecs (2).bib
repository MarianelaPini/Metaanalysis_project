@article{ WOS:A1993LP08700004,
Author = {LARBI, A and JABBAR, MA and ATTAKRAH, AN and COBBINA, J},
Title = {EFFECT OF TAKING A FODDER CROP ON MAIZE GRAIN-YIELD AND SOIL
   CHEMICAL-PROPERTIES IN LEUCAENA AND GLIRICIDIA ALLEY FARMING SYSTEMS IN
   WESTERN NIGERIA},
Journal = {EXPERIMENTAL AGRICULTURE},
Year = {1993},
Volume = {29},
Number = {3},
Pages = {317-321},
Month = {JUL},
Abstract = {The effect of managing hedgerow foliage for mulch and fodder in Leucaena
   leucocephala and Gliricidia sepium alley farming systems on maize grain
   yield and soil chemical characteristics was studied in on-station and
   on-farm experiments in southwestern Nigeria. Yields increased as the
   proportion of mulch applied increased but the extra increases obtained
   when more than half the foliage was applied as mulch were relatively
   small, suggesting that half the foliage may be removed as feed without
   detrimental effect. Soil organic carbon, nitrogen and available
   phosphorus tended to increase with increasing proportion of prunings
   applied as mulch.},
DOI = {10.1017/S0014479700020883},
